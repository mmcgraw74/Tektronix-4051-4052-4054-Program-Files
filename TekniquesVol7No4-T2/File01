1 REM AUTOLOAD DIRECTORY
2 GO TO 100
4 GO TO 160
100 INIT 
110 DATA 11,2,3,4,5,6,7,8,13,20,24,40
120 READ N
130 IMAGE 10T,2D,5X,FA
140 DIM F(N)
150 READ F
160 PRINT "       TEKTRONIX IDD PROGRAM EXCHANGE
"
170 PRINT "            TEKniques Vol. 7 No.4 T2 Tape
"
180 PRINT "      PROGRAM   TITLE
"
190 PRINT USING 130:1,"Multivariate Bargraph"
200 PRINT USING 130:2,"Log Axis"
210 PRINT USING 130:3,"INTERP"
220 PRINT USING 130:4,"LORAN-C Readings"
230 PRINT USING 130:5,"GPIB General Device Exerciser"
240 PRINT USING 130:6,"4050/1980 Interface"
250 PRINT "
CAUTION: TRANSFER THE FOLLOWING PROGRAM TO DISK."
260 PRINT USING 130:7,"BASIC Linker"
270 PRINT "
CAUTION: TRANSFER EACH OF THE FOLLOWING PROGRAMS "
280 PRINT "TO ITS OWN DEDICATED TAPE.  SEE DOCUMENTATION.
"
290 PRINT USING 130:8,"16K Data Graphing"
300 PRINT USING 130:9,"Stereonet Plot"
310 PRINT USING 130:10,"TM5000 Instrument Checkout"
320 PRINT USING 130:11,"4051 Assembler"
330 PRINT "
ENTER THE PROGRAM OF YOUR CHOICE OR 0 TO END. ";
340 INPUT P
350 P=INT(P)
360 IF P=0 THEN 500
370 IF P<1 OR P>N THEN 160
380 IF P<7 THEN 430
390 PRINT "
YOU CAN'T RUN THAT PROGRAM FROM THIS TAPE; YOU MUST "
400 PRINT "TRANSFER IT TO ITS OWN DEDICATED TAPE/DISK"
410 PRINT "
PRESS USER-DEFINABLE KEY 1 TO CHOOSE ANOTHER"
420 RETURN 
430 FIND F(P)
440 T=TYP(0)
450 IF T>2 THEN 480
460 FIND F(P)
470 OLD 
480 FIND F(P)
490 CALL "BOLD"
500 PRINT "
Goodbye.  If you change your mind, press UDK #1"
510 END 

