1 REM PLOT10e to R12 Creator - Sept 2023 Monty McGraw2 REM HxD export plt file as S19 3 REM Edit .s19 file with Notepad++, delete S19 address, checksum, CRLF4 REM to create single s19e ASCII HEX string up to max size of A$5 REM then search for ^.{16002} and replace with $0\r to break the file6 REM into 16002 byte strings.  Separate into separate 16KB files7 REM Run this program on each separate file, creating new ASCII HEX8 REM output files - which will be concatenated with a separate program100 INIT 105 DIM F(2),A$(16002),C$(8004),L$(24),T$(3),H(2,2)200 REM Convert PLOT-10 GRAPHICS to R12 graphics202 PRINT "PLOT10 to R12 ConverterInput PLT starting File#: ";203 INPUT F0230 G=0240 X0=0250 Y0=0260 X9=0270 Y9=0290 Z=0300 C$=""310 Q=1320 REM I=number of files to input and write until A$="|EOG|"330 I=0331 PAGE335 REM use Flash Drive file 100 for first Fast Graphics/R12 file336 REM increment output file number until done340 FIND@5:F0+I350 REM input entire plt.s19e file into A$352 INPUT@5:A$353 I$=SEG(A$,1,6)361 P=1362 FIND@5:100+I363 I=I+1364 IF A$<>"|EOG|" THEN 370365 WRITE @5:"|EOG|"366 PRINT ""367 END 370 GOSUB 720380 REM HIGH Y390 IF T>63 THEN 430400 Y9=T-32410 GOSUB 720420 REM LOW Y430 IF T<96 THEN 470440 Y0=T-96450 GO TO 410460 REM HIGH X470 IF T>63 THEN 510480 X9=T-32490 GOSUB 720500 REM LOW X510 X0=T-64520 X=(X9*32+X0)*0.128530 Y=(Y9*32+Y0)*0.128540 IF Z=0 THEN 590545 Z0=1550 PRINT @32,20:X,Y560 F(1)=X570 F(2)=Y580 GO TO 630590 PRINT @32,21:X,Y600 F(1)=-X-0.001*(X=0)610 F(2)=-Y-0.001*(Y=0)615 Z0=0620 Z=1630 CALL "CHANGE",F,T$631 C$=REP(T$,Q,0)632 Q=LEN(C$)+1633 IF Q<5001 THEN 659640 WRITE @5:C$641 C$=T$642 T$=SEG(C$,1,1)643 T0=ASC(T$)644 T0=T0+64*(T0<64)645 T$=CHR(T0)646 C$=REP(T$,1,1)658 Q=LEN(C$)+1659 IF P<LEN(A$) THEN 370660 REM keep last vector C$ & Q as MOVE for next file662 WRITE @5:C$664 C$=T$666 T$=SEG(C$,1,1)668 T0=ASC(T$)670 T0=T0+64*(T0<64)672 T$=CHR(T0)674 C$=REP(T$,1,1)676 Q=LEN(C$)+1680 GO TO 340690 G=1700 Z=0710 REM NEXT CHARACTER720 IF P>LEN(A$) THEN 662730 B$=SEG(A$,P,2)740 CALL "HEXDEC",B$,T745 T=T-128*(T>127)750 P=P+2760 IF G=1 AND T>31 THEN 840770 IF T=27 THEN 820780 IF T=29 THEN 690790 IF T<>31 THEN 720800 G=0810 GO TO 720820 P=P+2830 GO TO 720840 RETURN 3000 REM test RDRAW the R12 file if <16000 bytes3005 PAGE 3010 FIND @5:1003020 INPUT @5,19:Z$3030 Z$=SEG(Z$,21,16)3040 PRINT "";Z$3050 FIND @5:1003060 READ @5:A$3070 IF A$="|EOG|" THEN 40003080 CALL "RDRAW",A$,1,0,03090 GO TO 30604000 PRINT ""4010 END 