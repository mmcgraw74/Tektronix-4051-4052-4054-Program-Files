1 REM PLOT10 R12 Viewer - August 2022 Monty McGraw100 INIT 105 DIM C$(8004)110 PAGE 210 REM DRAW PLOT-10 GRAPHICS212 PRINT "Tektronix PLOT10 - R12 VIEWER - September 2023 mcm"214 PRINT "On Tektronix 4050 computers with R12/Fast Graphics ROM Pack"220 PRI@5,9:"ROOT"230 FIND@5:123240 INPUT@5:T1,N$,S9250 FOR I=1 to S9*5260 NEXT I280 REM change back to PLOT10-R12 directory290 PRI@5,9:"PLOT10-R12"300 For F0 = 9 to 213010 FIND @5:F03020 INPUT @5,19:Z$3030 Z$=SEG(Z$,21,16)3032 PAGE3035 MOVE 2,1003040 PRINT Z$3050 FIND @5:F03060 READ @5:C$3070 IF C$="|EOG|" THEN 40003080 CALL "RDRAW",C$,1,0,03090 GO TO 30604000 MOVE 0,1004001 REM don't PRINT Z$ again - text & cursor shifts after move to 0,1004004 FOR I=1 to S9*54005 NEXT I4010 Next F0 4020 INPUT A$4030 PRI@5,9:"ROOT"4040 FIND@5:14050 OLD@5: