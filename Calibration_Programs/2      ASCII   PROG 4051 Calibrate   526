1 INIT2 SET KEY3 GO TO 2004 PAGE5 MOVE 0,1006 PRINT A$7 GO TO 2108 PAGE9 MOVE 0,10010 PRINT A$11 GO TO 23040 PRI@5,9:"ROOT"41 FIND@5:142 OLD@5:200 A$="UDK1 Focus	UDK2 Defocus		UDK10 Flash Menu"201 PAGE202 MOVE 0,100203 PRINT A$;204 END209 REM Focus210 PAGE211 FOR I=1 to 70212 PRINT"@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@";214 NEXT I220 GO TO 202229 REM Deflection230 PAGE235 MOVE 0,0240 DRAW 130,0250 DRAW 130,100260 DRAW 0,100270 DRAW 0,0280 MOVE 65,0290 DRAW 65,100300 MOVE 0,50310 DRAW 130,50320 GOTO 202