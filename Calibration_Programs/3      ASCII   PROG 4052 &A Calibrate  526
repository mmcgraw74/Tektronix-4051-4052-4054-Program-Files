1 INIT2 SET KEY3 GO TO 2004 PAGE5 MOVE 0,1006 PRINT A$7 GO TO 2508 PAGE9 MOVE 0,10010 PRINT A$11 GO TO 27012 PAGE13 MOVE 0,10014 PRINT A$15 GO TO 38040 PRI@5,9:"ROOT"41 FIND@5:142 OLD@5:200 M=INT(100*RND(0))205 IF M=70 THEN 230210 A$="UDK1 Focus    UDK2 Deflection    UDK3 Vector Length"215 A$=A$&"	UDK10 Flash Menu"220 GO TO 240230 A$="UDK1 Focus    UDK2 Deflection		DK10 Flash Menu"235 DELETE 12,15240 PAGE242 MOVE 0,100243 PRINT A$;244 END249 REM Focus250 PAGE251 FOR I=1 to 70252 PRINT"@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@@";254 NEXT I260 GO TO 242269 REM Deflection270 PAGE275 MOVE 0,0280 DRAW 130,0290 DRAW 130,100300 DRAW 0,100310 DRAW 0,0320 DRAW 0,50330 DRAW 130,50340 MOVE 65,100350 DRAW 65,0360 GO TO 242 370 REM Vector Length Adjustment (4052A only)380 PRINT "13.7 MS VECTOR TIMER ADJUSTMENT PATTERN"390 PRINT "ADJUST FOR MINIMAL OVER AND UNDERSHOOT"400 PAGE405 FOR L1=0 TO 50 STEP 5*0.128410 MOVE L1,100-L1420 DRAW L1,L1430 DRAW 130-L1,L1440 DRAW 130-L1,100-L1450 DRAW L1,100-L1460 NEXT L1470 GO TO 242 