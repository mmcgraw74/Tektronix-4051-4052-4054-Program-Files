100 REM R12 Bitmap Viewer110 REM each READ contains I and a precomputed R12 dot string120 REM last READ contains "|EOG|" to stop viewer130 PRINT "R12 Bitmap ViewerInput Bitmap file#: ";140 INPUT F150 FIND @5:F160 PAGE 170 READ @5:I180 DELETE Z$190 IF I=0 THEN 250200 DIM Z$((I-1)*3)210 READ @5:Z$220 IF I<2 THEN 170230 CALL "RDOTS",Z$,1,0,0240 GO TO 170250 DIM Z$(5)260 READ @5:Z$270 IF Z$="|EOG|" THEN 290280 PRINT "Error not |EOG|"290 HOME 300 END 