1 GO TO 1004 SET NOKEY 5 GO TO 806 SET KEY 7 RETURN 20 SET NOKEY 21 GO TO 9022 SET KEY 23 RETURN 80 IF E2=0 THEN 8581 E2=082 GO TO 685 E2=-0.12*G286 GO TO 690 IF E2=0 THEN 9591 E2=092 GO TO 2295 E2=0.12*G296 GO TO 6100 INIT 105 GO TO 5000110 DIM H(2,25),V(2,25)120 E1=65130 E2=0140 S3=0150 S4=0200 REM210 C=1.1220 FOR T=1 TO 25230 MOVE 65,50240 ROPEN T250 VISIBILITY T,0260 RMOVE -0.05*C,-0.8*C270 RDRAW 0,0.8*C280 RDRAW 0.15*C,-0.05*C290 RDRAW -0.15*C,-0.05*C310 RCLOSE 320 MOVE 65,50330 C=C^1.17340 NEXT T350 C=1.1360 FOR T=26 TO 50370 MOVE 65,50380 ROPEN T390 VISIBILITY T,0400 RMOVE 0.05*C,-0.8*C410 RDRAW 0,0.8*C420 RDRAW -0.15*C,-0.05*C430 RDRAW 0.15*C,-0.05*C450 RCLOSE 460 C=C^1.17470 NEXT T500 REM520 Y1=40530 Y2=40540 Q=1.01550 FOR T=1 TO 25560 STPOINT T,X1,Y1570 H(1,T)=X1580 V(1,T)=Y1590 X1=X1+(Q-1)*W1600 Y1=Y1-(Q-1)*0.2610 Q=Q^1.28620 NEXT T630 Q=1.01640 FOR T=26 TO 50650 STPOINT T,X2,Y2660 H(2,T-25)=X2670 V(2,T-25)=Y2680 X2=X2-(Q-1)*W1690 Y2=Y2-(Q-1)*0.2700 Q=Q^1.28710 NEXT T1000 REM CAR1010 MOVE 65,11020 ROPEN 1001030 RMOVE -15,01040 RDRAW 2,41050 RDRAW 3,41060 RDRAW 2,1.51070 RDRAW 2,0.81080 RDRAW 2,0.41090 RDRAW 1.5,0.21100 RDRAW 1.3,0.11110 RDRAW 2,01120 RDRAW 1.3,-0.11130 RDRAW 1.5,-0.21140 RDRAW 1.8,-0.41150 RDRAW 2,-0.81160 RDRAW 2,-1.51170 RDRAW 3,-41180 RDRAW 2,-41190 RMOVE 1,01200 RDRAW 0.6,31210 RDRAW 0.5,11220 RDRAW 0.8,0.51230 RDRAW 5,01240 RDRAW 0.8,-0.51250 RDRAW 0.8,-1.51260 RDRAW 0.6,-2.51270 RDRAW 0.1,-11280 RMOVE -5,01290 RDRAW -0.1,11300 RDRAW -0.6,2.51310 RDRAW -0.8,1.51320 RDRAW -0.8,0.51330 RMOVE -33,-4.51340 RDRAW -0.6,31350 RDRAW -0.5,11360 RDRAW -0.8,0.51370 RDRAW -0.8,-0.51380 RDRAW -0.8,-1.51390 RDRAW -0.6,-2.51400 RDRAW -0.1,-11410 RMOVE -5,01420 RDRAW 0.1,11430 RDRAW 0.6,2.51440 RDRAW 0.8,1.51450 RDRAW 0.8,0.51460 RDRAW 5,01470 RCLOSE 1480 MOVE 65,11500 REM EXPLOSION1510 MOVE 65,501520 ROPEN 1101530 VISIBILITY 110,01540 C=11550 GOSUB 17001560 RCLOSE 1570 STPOINT 110,0,1001600 MOVE 65,501610 ROPEN 1111620 VISIBILITY 111,01630 C=-11640 GOSUB 17001650 RCLOSE 1660 STPOINT 111,130,1001670 GO TO 30001700 REM DRAW EXPLOSION1705 RMOVE -10*C,01710 RDRAW -6*C,-31720 RDRAW 3*C,2.41730 RDRAW -15*C,0.61740 RDRAW 12*C,31750 RDRAW -4.5*C,31760 RDRAW 4.5*C,01770 RDRAW -6*C,91780 RDRAW 6*C,-31790 RDRAW 0,31800 RDRAW 6*C,-61810 RDRAW 1.5*C,31820 RDRAW 1.5*C,-4.51830 RDRAW 1.5*C,1.51840 RDRAW -1.5*C,-61850 RETURN 2000 REM MOVE FENCE POSTS2001 SET KEY 2005 C=1.12010 D=RND(-1)-0.52020 D=SGN(D)2030 D1=(RND(-1) MAX 0.4)*D*D22040 S1=12050 S2=S1-G3 MAX 12060 FOR W=S2 TO S12070 H(1,W)=H(1,W)+D1*C2080 H(2,W)=H(2,W)+D1*C2090 STPOINT W,H(1,W),V(1,W)2100 STPOINT W+25,H(2,W),V(2,W)2105 GOSUB 24002110 NEXT W2115 C=C^(1.16-G3*0.01)2120 S1=S1+1 MIN 252130 IF S1<25 THEN 20502140 S2=S2+12150 IF S2=>26 THEN 21702160 GO TO 20602170 IF H(1,25)>G1 THEN 22002180 IF H(2,25)<G2 THEN 22402190 GO TO 20002200 D=SGN(D1)2205 C=1.12210 IF D=-1 THEN 20402220 D1=D1*-12230 GO TO 20402240 D=SGN(D1)2245 C=1.12250 IF D=1 THEN 20402260 D1=D1*-12270 GO TO 20402400 REM CAR2450 E1=E1+E2 MAX 20 MIN 1102460 STPOINT 100,E1,12462 S4=S4+0.1052465 IF S4=>130 THEN 45002468 STPOINT 102,S4,932470 IF E1<H(2,25)+26 THEN 26002480 IF E1>H(1,25)-26 THEN 27002482 S3=S3+G2*0.0072485 STPOINT 101,S3,872490 STPOINT 110,0,1002500 STPOINT 111,130,1002590 RETURN 2600 STPOINT 110,H(2,25)+10,22605 STPOINT 111,130,1002610 VISIBILITY 110,12690 GO TO 25902700 STPOINT 111,H(1,25)-10,22705 STPOINT 110,0,1002710 VISIBILITY 111,12790 GO TO 25903000 FOR T=1 TO 253010 VISIBILITY T,13020 VISIBILITY T+25,13030 IF T<>25 THEN 30803040 VISIBILITY 110,13050 VISIBILITY 111,13060 BLINK 110,O1,O23070 BLINK 111,O1,O23080 BLINK T,O1,O23090 BLINK T+25,O1,O23100 FOR T1=1 TO L13110 NEXT T13120 NEXT T3130 GO TO 20003500 REM VARIABLES FOR DIFFICULTY3510 CHARSIZE 43520 PRINT "Enter B for Beginner"3530 PRINT "      I for Intermediate"3540 PRINT "   or A for Advanced ";3550 INPUT P$3560 IF P$<>"B" AND P$<>"I" AND P$<>"A" THEN 35003580 IF P$="A" THEN 38003590 IF P$="I" THEN 37003600 X1=673605 REM G2 SETS MIN POST LIMIT,CAR H. SPEED,SCORE3610 X2=633620 G1=1253630 G2=53635 G3=23640 O1=0.253650 O2=0.83660 L1=4003670 W1=0.93680 D2=0.53690 GO TO 38903700 X1=66.53710 X2=63.53720 G1=1203730 G2=103735 G3=13740 O1=0.053750 O2=0.33760 L1=1003770 W1=0.843780 D2=0.753790 GO TO 38903800 X1=65.73810 X2=64.33820 G1=1103830 G2=153835 G3=03840 O1=0.053850 O2=0.153860 L1=703870 W1=0.753880 D2=13890 REM3900 PAGE 3910 MOVE 0,903920 DRAW 130,903930 MOVE 61,953940 PRINT "TIME"3950 MOVE 60,803960 PRINT "SCORE"3970 MOVE 10,873975 ROPEN 1013980 RDRAW -1,-33990 RDRAW 2,04000 RDRAW -1,34010 RCLOSE 4020 MOVE 10,934030 ROPEN 1024040 RDRAW -1,34050 RDRAW 2,04060 RDRAW -1,-34070 RCLOSE 4080 STPOINT 101,0,874090 STPOINT 102,0,934095 CHARSIZE 14100 FOR T=0 TO 1304110 MOVE T,904120 IF T/10=INT(T/10) THEN 41704130 RMOVE 0,24140 RDRAW 0,-44150 GO TO 43004170 RMOVE 0,34180 RDRAW 0,-64190 IF T=0 OR T=130 THEN 43004195 RMOVE -3,-24200 PRINT T/10;4220 PRINT "000"4300 NEXT T4310 GO TO 1104500 REM STOP HERE4510 FOR T=1 TO 2004520 FIX T4530 NEXT T4540 RINIT 4550 MOVE 42,554560 CHARSIZE 44570 PRINT "Press RETURN to try again";4580 INPUT P$4590 PAGE 4600 GO TO 35005000 REM5010 CHARSIZE 45015 PAGE 5020 PRINT "

