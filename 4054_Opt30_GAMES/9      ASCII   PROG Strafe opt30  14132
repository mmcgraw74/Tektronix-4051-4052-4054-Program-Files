1 GO TO 50004 D=D*-15 GOSUB 527 RETURN 8 S=S+0.05 MIN 1.511 RETURN 12 IF A6>0 THEN 1513 A6=214 GOSUB 9015 RETURN 16 S=S-0.05 MAX 0.519 RETURN 20 GO TO 423 RETURN 24 IF J1=1 THEN 601025 GOSUB 250026 RETURN 28 GO TO 10029 RETURN 32 IF A6>0 THEN 3533 A6=234 GOSUB 8035 RETURN 40 M=041 RETURN 50 REM51 IF D4/10<>INT(D4/10) THEN 5952 VISIBILITY A5,053 IF D<0 THEN 6554 A5=D4*0.155 STPOINT A5,D5,D656 IF D5<1 OR D5>129 OR D6>99 THEN 5957 VISIBILITY A5,159 RETURN 65 REM66 A5=D4*0.1+3767 STPOINT A5,D5,D668 IF D5<1 OR D5>129 OR D6>99 THEN 7069 VISIBILITY A5,170 RETURN 80 IF D<0 THEN 8682 D4=D4+2.583 IF D4<360 THEN 8584 D4=D4-36085 GO TO 5086 D4=D4-2.587 IF D4>0 THEN 8988 D4=D4+36089 GO TO 5090 IF D<0 THEN 9691 D4=D4-2.592 IF D4>0 THEN 9593 D4=D4+36095 GO TO 5096 D4=D4+2.597 IF D4<360 THEN 9998 D4=D4-36099 GO TO 50100 REM105 SET DEGREES 106 M=1107 Q4=0108 J1=0110 DIM H(129),V(129),B(10),U(129),F(9)115 A6=0120 R=170130 FOR T=1 TO 72140 VISIBILITY T,0150 NEXT T200 REM ** CALCULATE THE LAND210 L1=INT(RND(-1)*20)220 L2=INT(RND(-1)*50+25)230 L3=INT(RND(-1)*40+85)240 L4=RND(-1)*30250 L5=RND(-1)*30260 L6=RND(-1)*30270 FOR T=1 TO 129280 H(T)=T290 V(T)=SIN(ABS(L1-T))*L4+SIN(ABS(L2-T))*L5+SIN(ABS(L3-T))*L6+RND(-1)295 NEXT T300 REM ** CALCULATE BUILDINGS310 FOR T=1 TO 9 STEP 2320 B(T)=INT(RND(-1)*18)+12*T330 B(T+1)=RND(-1)335 V(B(T)-3)=V(B(T))340 V(B(T)-2)=V(B(T))355 V(B(T)-1)=V(B(T))360 V(B(T)+1)=V(B(T))365 V(B(T)+2)=V(B(T))370 V(B(T)+3)=V(B(T))375 F(T)=0380 NEXT T400 REM ** AIRCRAFT VARIABLES-W=WIND,S=STARTING SPEED,A=ALTITUDE,D=DIR404 REM  START NEW GAME IF NO TARGETS(B(T)'s=0)405 IF B(1)+B(3)+B(5)+B(7)+B(9)=0 THEN 100410 W=INT(RND(-1)*101)-50420 S=RND(-1)*0.5+0.4430 A=RND(-1)*36+60440 D=RND(-1)450 IF D<0.5 THEN 480460 D=-1465 A5=63470 GO TO 490480 D=1485 A5=9490 REM ** GOS TO DRAW LAND500 GOSUB 2500505 VISIBILITY A5,1510 REM ** MAIN PROGRAM530 IF D<0 THEN 600535 D4=90540 MOVE 5