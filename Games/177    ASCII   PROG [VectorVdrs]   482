100 INIT 105 DIM V(96),V$(432),V1(6)110 DATA 0,5,3,0,6,5120 READ V1130 FOR Y=1 TO 2140 FOR X=1 TO 8150 V(X*6-5+(Y-1)*48)=-(V1(1)+10*(X-1))160 V(X*6-4+(Y-1)*48)=-(V1(2)+10*(Y-1))170 V(X*6-3+(Y-1)*48)=V1(3)+10*(X-1)180 V(X*6-2+(Y-1)*48)=V1(4)+10*(Y-1)190 V(X*6-1+(Y-1)*48)=V1(5)+10*(X-1)200 V(X*6-0+(Y-1)*48)=V1(6)+10*(Y-1)210 NEXT X220 NEXT Y230 CALL "CHANGE",V,V$240 FOR Y=50 TO 10 STEP -10245 FOR X=10 TO 50 STEP 1250 CALL "RDRAW",V$,-1,X,Y260 NEXT X270 NEXT Y