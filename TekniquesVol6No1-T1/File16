1510 PRINT "            PART II:     LEADED COMPONENTS                 "
1520 PRINT 
1530 PRINT 
1540 PRINT 
1550 PRINT 
1560 PRINT 
1570 L$="leaded components"
1580 K$=L$
1590 GOSUB 5000
1600 N6=N
1610 IF N6=0 THEN 1800
1620 PRINT "Enter the approx. height of solder meniscus (after reflow) "
1630 INPUT H1
1640 PRINT 
1650 PRINT "Enter the PC board thickness"
1660 INPUT T1
1670 PRINT 
1680 PRINT "For each component, enter its part no., lead diameter, # of"
1690 PRINT "leads that are flush mounted, and the # of leads that are "
1700 PRINT "soldered through "
1710 DIM O(N6,7)
1720 FOR I=1 TO N6
1730 INPUT O(I,1),O(I,2),O(I,3),O(I,4)
1740 NEXT I
1750 FOR I=1 TO N6
1760 O(I,5)=O(I,3)*S2*PI*O(I,2)*H1
1770 O(I,6)=O(I,4)*S2*PI*O(I,2)*(H1+T1)
1780 O(I,7)=O(I,5)+O(I,6)
1790 NEXT I
1800 GO TO 200
1810 PAGE 
1820 PRINT "Has data been previously saved for this part ?             "
1830 PRINT "                                             1) yes        "
1840 PRINT "                                             2) no         "
1850 INPUT Q1
1860 IF Q1=2 THEN 2050
1870 PRINT 
1880 PRINT "Which file should it be retrieved from ?                   "
1890 INPUT A2
1900 FIND A2
1910 INPUT @33:R$
1920 INPUT @33:P$
1930 INPUT @33:I$
1940 INPUT @33:S$
1950 INPUT @33:E$
1970 INPUT @33:T$
1980 INPUT @33:N1,N2,N3,N4,N5,N6,T,E1
1990 DIM R(N1,16),C(N2,16),B(N3,16),F(N4,16),G(N5,16)
2000 INPUT @33:R,C,B
2010 INPUT @33:F,G
2020 IF N6=0 THEN 2050
2025 INPUT @33:L$
2030 DIM O(N6,7)
2040 INPUT @33:O
2050 K$=R$
2060 DELETE K,N
2070 N=N1
2080 IF N1=0 THEN 2120
2090 DIM K(N1,16)
2100 K=R
2110 GOSUB 7000
2120 K$=P$
2130 DELETE K,N
2140 N=N2
2150 IF N2=0 THEN 2190
2160 DIM K(N2,16)
2170 K=C
2180 GOSUB 7000
2190 K$=I$
2200 DELETE K,N
2210 N=N3
2220 IF N3=0 THEN 2260
2230 DIM K(N3,16)
2240 K=B
2250 GOSUB 7000
2260 K$=S$
2270 DELETE K,N
2280 N=N4
2290 IF N4=0 THEN 2330
2300 DIM K(N4,16)
2310 K=F
2320 GOSUB 7000
2330 K$=E$
2340 DELETE K,N
2350 N=N5
2360 IF N5=0 THEN 2410
2370 DIM K(N5,16)
2380 K=G
2390 GOSUB 7000
2400 GO TO 200
2410 K$=L$
2420 DELETE K,N
2430 N=N6
2440 IF N6=0 THEN 2480
2450 DIM K(N6,7)
2460 K=O
