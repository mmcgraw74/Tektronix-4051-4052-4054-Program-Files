5000 REMARK:  SUBROUTINE 1 --- NO. OF COMPONENTS
5010 DELETE K,N
5020 PRINT "Enter the number of ";K$
5030 INPUT N
5040 RETURN 
5050 REMARK:  SUBROUTINE 2 --- COMPONENT PARAMETERS
5055 PRINT "For each of the ";K$;", enter part no., L1, L2, W and L4   "
5060 DIM K(N,16)
5070 FOR I=1 TO N
5080 INPUT K(I,1),K(I,2),K(I,3),K(I,4),K(I,5)
5090 NEXT I
5100 FOR I=1 TO N
5110 K(I,6)=K(I,3)*K(I,4)
5120 K(I,7)=Z*S1*K(I,6)
5130 K(I,8)=Z*S2*K(I,6)
5140 NEXT I
5150 PRINT 
5160 PRINT "How many ";K$;" are mounted perpendicular to the axis of   "
5170 PRINT "the PC board ?"
5180 INPUT N7
5190 IF N7=0 THEN 5330
5200 PRINT 
5210 PRINT "Enter part no. of these ";K$;" and hit the return key after"
5220 PRINT "each one "
5230 FOR J=1 TO N7
5240 INPUT M1
5250 FOR I=1 TO N
5260 IF K(I,1)=M1 THEN 5290
5270 NEXT I
5280 GO TO 5330
5290 M2=K(I,4)
5300 K(I,4)=K(I,2)
5310 K(I,2)=M2
5320 NEXT J
5325 IF M3=2 THEN 5440
5330 FOR I=1 TO N
5340 K(I,9)=K(I,5)+K(I,2)/2
5350 K(I,10)=K(I,5)+K(I,2)
5360 K(I,11)=3.2*D1*(L3^3-6*L3*K(I,9)^2+4*K(I,9)^3)/L3^4
5370 K(I,12)=3.2*D1*K(I,9)*(L3^3-2*L3*K(I,9)^2+K(I,9)^3)/L3^4
5380 K(I,13)=3.2*D1*K(I,5)*(L3^3-2*L3*K(I,5)^2+K(I,5)^3)/L3^4
5390 K(I,14)=3.2*D1*K(I,10)*(L3^3-2*L3*K(I,10)^2+K(I,10)^3)/L3^4
5400 K(I,15)=K(I,12)-K(I,11)*K(I,2)/2-K(I,13)
5410 K(I,16)=K(I,12)+K(I,11)*K(I,2)/2-K(I,14)
5420 NEXT I
5430 GO TO 5540
5440 FOR I=1 TO N
5450 K(I,9)=K(I,5)+K(I,2)/2
5460 K(I,10)=K(I,5)+K(I,2)
5470 K(I,11)=16*D1*(2*K(I,9)*L3^2-6*L3*K(I,9)^2+4*K(I,9)^3)/L3^4
5480 K(I,12)=16*D1*K(I,9)^2*(L3-K(I,9))^2/L3^4
5490 K(I,13)=16*D1*K(I,5)^2*(L3-K(I,5))^2/L3^4
5500 K(I,14)=16*D1*K(I,10)^2*(L3-K(I,10))^2/L3^4
5510 K(I,15)=K(I,12)-K(I,11)*K(I,2)/2-K(I,13)
5520 K(I,16)=K(I,12)+K(I,11)*K(I,2)/2-K(I,14)
5530 NEXT I
5540 RETURN 
6000 REMARK:  SUBROUTINE 3 --- IC PARAMETERS
6010 PRINT "For each IC, enter its part no., L1, A, Z5 (# of pads), L4 "
6020 DIM K(N,16)
6030 FOR I=1 TO N
6040 INPUT K(I,1),K(I,2),K(I,3),K(I,4),K(I,5)
6050 NEXT I
6060 FOR I=1 TO N
6070 K(I,6)=K(I,4)*S1*K(I,3)
6080 K(I,7)=K(I,6)
6090 K(I,8)=K(I,4)*S2*K(I,3)
6100 K(I,9)=K(I,5)+K(I,2)/2
6110 K(I,10)=K(I,5)+K(I,2)
6120 NEXT I
6125 IF M3=2 THEN 6220
6130 FOR I=1 TO N
6140 K(I,11)=3.2*D1*(L3^3-6*L3*K(I,9)^2+4*K(I,9)^3)/L3^4
6150 K(I,12)=3.2*D1*K(I,9)*(L3^3-2*L3*K(I,9)^2+K(I,9)^3)/L3^4
6160 K(I,13)=3.2*D1*K(I,5)*(L3^3-2*L3*K(I,5)^2+K(I,5)^3)/L3^4
6170 K(I,14)=3.2*D1*K(I,10)*(L3^3-2*L3*K(I,10)^2+K(I,10)^3)/L3^4
6180 K(I,15)=K(I,12)-K(I,11)*K(I,2)/2-K(I,13)
6190 K(I,16)=K(I,12)+K(I,11)*K(I,2)/2-K(I,14)
6200 NEXT I
6210 GO TO 6300
6220 FOR I=1 TO N
6230 K(I,11)=16*D1*(2*K(I,9)*L3^2-6*L3*K(I,9)^2+4*K(I,9)^3)/L3^4
6240 K(I,12)=16*D1*K(I,9)^2*(L3-K(I,9))^2/L3^4
6250 K(I,13)=16*D1*K(I,5)^2*(L3-K(I,5))^2/L3^4
6260 K(I,14)=16*D1*K(I,10)^2*(L3-K(I,10))^2/L3^4
6270 K(I,15)=K(I,12)-K(I,11)*K(I,2)/2-K(I,13)
6280 K(I,16)=K(I,12)+K(I,11)*K(I,2)/2-K(I,14)
6290 NEXT I
6300 RETURN 
7000 REMARK:  SUBROUTINE 4 --- PRINTOUT (LEADLESS)
7010 PAGE 
7020 PRINT USING 7030:T$
7030 IMAGE 25T, 55A
7040 PRINT USING 7030:"(STATIC LOADING)"
7050 PRINT 
7060 PRINT 
7070 PRINT 
7080 PRINT USING 7090:K$
7090 IMAGE 25T,55A
7100 PRINT 
7110 A$="PART NO."
7120 B$="TENSILE FORCE (LB)"
7130 C$="SHEAR FORCE (LB)"
7140 D$="BENDING FAILURE?"
7150 Y$="YES"
7160 PRINT USING 7170:A$,B$,C$,D$
7170 IMAGE 8A,4X,18A,4X,16A,4X,16A
7180 PRINT 
7185 D3=T*E1/100
7190 FOR I=1 TO N
7200 IF K(I,16)>D3 THEN 7250
7210 PRINT USING 7220:K(I,1),K(I,7),K(I,8)
7220 IMAGE 5D,6X,5D.2D,14X,5D.2D
7230 NEXT I
7240 GO TO 7280
7250 PRINT USING 7260:K(I,1),K(I,7),K(I,8),Y$
7260 IMAGE 5D,6X,5D.2D,14X,5D.2D,15X,4A
7270 NEXT I
7280 MOVE 85,0
7290 PRINT "Hit page to continue "
7300 RETURN 
8000 REMARK:  SUBROUTINE 5 --- PRINTOUT (LEADED)
8010 PAGE 
8020 PRINT USING 7030:T$
8030 PRINT USING 7030:"(STATIC LOADING)"
8040 PRINT 
8050 PRINT 
8060 PRINT 
8070 PRINT 
8080 PRINT 
8090 PRINT USING 7090:K$
8100 PRINT 
8110 A$="PART NO."
8120 C$="NORMAL FORCE (LB)"
8130 PRINT 
8140 F$="NO. OF LEADS"
8150 PRINT USING 8160:A$,F$,C$
8160 IMAGE 8A,4X,12A,4X,17A
8170 PRINT 
8180 FOR I=1 TO N
8190 PRINT USING 8200:K(I,1),K(I,3)+K(I,4),K(I,7)
8200 IMAGE 5D,7X,5D,14X,5D.2D
8210 NEXT I
8220 MOVE 85,0
8230 PRINT "Hit page to continue "
8240 RETURN 

