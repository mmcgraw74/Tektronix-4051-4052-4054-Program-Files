180 L$=" "
190 Q$=CHR(13)
200 RETURN 
210 GOSUB 6100
220 PRINT "
   Press <RETURN> to create a pie chart";
230 INPUT B$
240 PAGE 
250 PRINT "
  How many PIE segments:  ";
260 INPUT N
270 Q=N
280 C=11
290 DIM A$(C*Q),D(Q),C3(Q+1),S1(Q),E(Q)
300 A$=""
310 S=0
320 S$=""
330 T$=""
340 S1=-45
350 V$="B"
360 E=0
400 REM ....................DATA INPUT SECTION
410 FOR I=1 TO N
420 PAGE 
430 PRINT "Segment #";I
440 PRINT USING """
Enter text : "",11""_"",11"""",S":
450 B$="               "
460 INPUT K$
470 B$=REP(K$,1,C)
480 B$=B$&"                                  "
490 B$=SEG(B$,1,C)
500 A$=A$&B$
510 PRINT "
Enter value : ";
520 INPUT B
530 IF B<=0 THEN 510
540 D(I)=B
550 S=S+D(I)
560 PRINT "
Explode this segment? (Y/N_): ";
570 INPUT B$
580 IF B$="" OR B$="N" THEN 600
590 E(I)=1
600 PRINT "
Shade this segment? (Y/N_): ";
610 INPUT B$
620 IF B$="" OR B$="N" THEN 770
630 PRINT "
  H   Horizontal"
640 PRINT "  V   Vertical"
650 PRINT "  D   Diagonal"
660 PRINT "  N   None"
670 PRINT "
Enter type of shading : ";
680 INPUT B$
690 S1(I)=-45*(B$="N")+-45*(B$="")+(B$="H")*0+(B$="D")*45+(B$="V")*90
700 IF S1(I)<0 THEN 770
710 PRINT "
Enter number of shading lines: ";
720 INPUT B$
730 B$=B$&" 0"
740 O3=VAL(B$)
750 IF O3<=0 OR O3>25 THEN 710
760 S1(I)=S1(I)+O3
770 NEXT I
780 GOSUB 810
790 GOSUB 6100
800 END 
810 PAGE 
820 PRINT "
Do you want segment values printed as:"
