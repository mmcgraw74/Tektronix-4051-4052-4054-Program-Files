7410 D=(C(7)-X2)/N1
7420 GO TO 7490
7430 PRINT "CELL WIDTH ";
7440 INPUT D
7450 GO TO 7480
7460 PRINT "OFFSET, CELL WIDTH ";
7470 INPUT X2,D
7480 N1=INT((C(7)-X2)/D)+1
7490 DELETE E
7500 DIM E(N1)
7510 E=0
7520 PRINT "
DO YOU WANT NORMAL CURVE ";
7530 INPUT E$
7540 C1=X2+N1*D
7550 IF N<C(1) THEN 7580
7560 GOSUB 7610
7570 GO TO 7670
7580 I1=1
7590 GOSUB 7150
7600 GO TO 7670
7610 FOR J=1 TO N
7620 X1=INT((A0(J)-X2)/D)+1-(A0(J)=C1)
7630 IF X1<1 OR X1>N1 THEN 7650
7640 E(X1)=E(X1)+1
7650 NEXT J
7660 RETURN 
7670 N2=E(1)
7680 FOR I=1 TO N1
7690 N2=N2 MAX E(I)
7700 NEXT I
7710 N2=INT(1.1*N2)+2
7720 X1=X2-(C1-X2)*0.2
7730 X4=C1+(C1-X2)*0.076924
7740 X3=X1+0.42*(X2-X1)
7750 Y1=-N2*0.081396
7760 Y4=N2*1.09
7770 PAGE 
7780 PRINT "                      DATA I.D.: ";
7790 PRINT I$
7800 PRINT "
                     DATE: ";
7810 PRINT D$
7820 PRINT "
                     MEASUREMENT: ";
7830 GOSUB S(X) OF 6720,6740,6760,6780,6800,6820
7840 HOME 
7850 WINDOW X1,X4,Y1,Y4
7860 VIEWPORT 0,130,0,100
7870 T=X2
7880 D7=N2*0.007
7890 D5=1+INT(N1*0.1373076923)
7900 D1=INT(LGT(C1 MAX ABS(X2))+1.0E-8)
7910 D2=10^-D1
7920 T3=-N2*0.04069767442
7930 D4=(C1-X2)*0.05149038462
7940 MOVE X3,0
7950 DRAW C1,0
7960 FOR I=0 TO N1
7970 MOVE T,D7
7980 D6=INT(I/D5)=I/D5
7990 DRAW T,-D6*D7
8000 IF D6=0 THEN 8030
8010 MOVE T-D4,T3
8020 PRINT USING "+D.3D,S":T*D2
8030 T=T+D
8040 NEXT I
8050 MOVE (C1+X2)/2-D4/2,Y1
8060 PRINT "*10^";D1;
8070 T=0
8080 D7=(C1-X2)*0.007
8090 D5=1+INT(N2*0.0651162791)
8100 MOVE X3,0
8110 DRAW X3,N2
8120 FOR I=0 TO N2
8130 MOVE X3+D7,I
8140 D6=INT(I/D5)=I/D5
8150 DRAW X3-D6*D7,I
8160 IF D6=0 THEN 8190
8170 MOVE X1,I
8180 PRINT USING "4D,4D.D":I,I*100/C(1)
8190 NEXT I
8200 MOVE X1,N2*1.045
8210 PRINT "  N   % REL FREQ"
8220 FOR I=1 TO N1
8230 MOVE X2+(I-1)*D,0
8240 DRAW X2+(I-1)*D,E(I)
8250 DRAW X2+I*D,E(I)
8260 DRAW X2+I*D,0
8270 NEXT I
8280 E$=SEG(E$,1,1)
8290 IF E$="N" THEN 8370
8300 GOSUB 8450
8310 D6=(C1-X2)/50
8320 D5=D*C(1)/SQR(2*PI*X1)
8330 MOVE X2,D5*EXP((X2-Y1)*(Y1-X2)/2/X1)
8340 FOR I=X2 TO C1 STEP D6
8350 DRAW I,D5*EXP((I-Y1)*(Y1-I)/2/X1)
8360 NEXT I
8370 HOME 
8380 N=Q1
8390 T=Q2
8400 D=Q3
8410 D3=Q4
8420 C1=Q5
8430 REM*********GLOBAL PARAMETERS RESTORED
8440 GO TO 3830
8450 Y1=C(2)/C(1)
8460 X1=(C(3)-C(2)*Y1)/(C(1)-1)
8470 RETURN 
8480 REM******************READ DATA TAPE
8490 ON EOF (0) THEN 8620
8500 PAGE 
8510 PRINT "ENTER FILE #: "
8520 INPUT X
8530 FIND X
8540 INPUT @33:A$,B$
8550 PRINT A$
8560 PRINT B$
8570 INPUT @33:C$
8580 PRINT C$
8590 INPUT @33:X
8600 PRINT X
8610 GO TO 8570
8620 END 
8630 REM********************SUBROUTINE INTERACTION 1*************
8640 PAGE 
8650 PRINT USING "7X,2D":P
8660 PRINT "INTERACTION  "
8670 F1=1
8680 FOR I=1 TO 2
8690 PRINT ""
8700 CALL "WAIT",0.25
8710 NEXT I
8720 FOR J8=1 TO 2
8730 J9=U9(J8)
8740 GOSUB J9 OF 6110,4100,4990,5540,2130
8750 V9(J8)=D9
8760 NEXT J8
8770 IF G$="+" THEN 8810
8780 IF G$="-" THEN 8830
8790 IF G$="*" THEN 8850
8800 IF G$="/" THEN 8870
8810 D9=V9(1)+V9(2)
8820 GO TO 8890
8830 D9=V9(1)-V9(2)
8840 GO TO 8890
8850 D9=V9(1)*V9(2)
8860 GO TO 8890
8870 D9=V9(1)/V9(2)
8880 N$="N6"
8890 F1=0
8900 HOME 
8910 PRINT USING "20(L),FA,1D,S":"INTERACTION: ",U9(1)
8920 PRINT G$;
8930 PRINT U9(2)
8940 PRINT "

"
8950 PRINT USING "9D.3D":D9
8960 PRINT "

MAKE SELECTION"
8970 PRINT "FROM TABLET MENU"
8980 GOSUB 4730
8990 IF W=1 THEN 8630
9000 IF W=2 THEN 9030
9010 IF W=3 THEN 9050
9020 IF W=4 THEN 9070
9030 GOSUB 2760
9040 RETURN 
