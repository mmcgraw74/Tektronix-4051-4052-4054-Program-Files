100 INIT102 REM rotate Michelangelo left 90deg and swap "X" left and right103 VIEWPORT 32.5,130-32.5,0,100105 WINDOW 0,200,0,320110 PAGE120 FIND@5:40130 INPUT@5:A$140 INPUT@5:A$145 L=0150 INPUT@5:A$155 L=L+2160 B$=SEG(A$,1,1)170 IF B$="E" THEN 320180 INPUT@5: Y0,X0,Y,X190 X8=X0200 Y8=Y0210 MOVE X0,Y0220 DRAW X,Y230 IF X<>X8 OR Y<>Y8 THEN 260240 REM PRINT "X"250 GO TO 150260 INPUT@5:A$265 L=L+2270 INPUT@5: Y1,X1,Y,X280 MOVE X1,Y1290 DRAW X,Y300 GO TO 230310 GO TO 150320 HOME330 END