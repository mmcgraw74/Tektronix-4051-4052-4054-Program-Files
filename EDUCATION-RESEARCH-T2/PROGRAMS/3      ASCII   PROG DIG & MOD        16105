),Y1(K,J1)860 NEXT J1870 IF K=A THEN 890880 PRINT "Repeat Frame/Image ";I;" again"890 NEXT K900 FOR J2=1 TO P910 X(I,J2)=0920 Y(I,J2)=0930 FOR J3=1 TO A940 X(I,J2)=X(I,J2)+X1(J3,J2)950 DELETE 16,22960 Y(I,J2)=Y(I,J2)+Y1(J3,J2)970 NEXT J3980 X(I,J2)=X(I,J2)/A990 Y(I,J2)=Y(I,J2)/A1000 NEXT J21010 N=I1020 PRINT "FRAME/IMAGE ";I;" COMPLETE"1030 NEXT I1040 DELETE X1,Y1,T1,N1,N91050 GO TO 2301060 REMARK - DIGITIZE WITH AVERAGING *********1070 DIM X(N9,P),Y(N9,P),T(N9)1080 IF S$<>"Y" THEN 11001090 DELETE 730,10501100 PRINT "Begin Digitizing"1110 LET N1=11120 FOR I=N1 TO N91130 INPUT @14,32:T(I),T11140 IF T(I)>99 THEN 12301150 T(I)=INT(T(I))1160 PRINT I,T(I)1170 FOR J=1 TO P1180 INPUT @14,32:X(I,J),Y(I,J)1190 NEXT J1200 PRINT "FRAME/IMAGE ";I;" COMPLETE"1210 N=I1220 NEXT I1230 PRINT "IS THIS A CORRECTION (Y OR N):";1240 INPUT G$1250 IF G$="N" THEN 13101260 PRINT "INPUT NEW STARTING POINT: ";1270 INPUT N11280 PRINT "Begin Digitizing Frame/Image ";N1;" again"1290 IF A=1 THEN 11201300 GO TO 7801310 PRINT "DATA ENTERED INTO MEMORY"1320 PRINT "INSERT PROGRAM TAPE"1330 PRINT "Press 'RETURN' to continue";1340 INPUT G$1350 GO TO 2001400 REMARK - STORE DATA **********1410 PRINT "STORE THE DATA ON TAPE"1420 PRINT "INSERT DATA TAPE"1430 PRINT "SOURCE OF DATA TAPE: TAPE(33) OR Flash Drive(5) ";1440 INPUT P31450 PRINT "STATED ID IS:  ";I$1460 PRINT "Is this the correct ID. ( Y or N ): ";1470 INPUT G$1480 IF G$="Y" THEN 15201490 IF G$<>"N" THEN 14501500 PRINT "New correct ID. = ";1510 INPUT I$1520 PRINT "DO YOU WISH TO CORRECT FOR TIME FACTOR? (Y OR N): ";1530 INPUT G$1540 IF G$="Y" THEN 25401550 PRINT "IS DATA TO BE STORED IN (A):SINGLE FILE OR "1560 PRINT "                        (B):SEPARATE FILES  ? ";1570 INPUT G$1580 IF G$="B" THEN 19901590 PRINT "INPUT FILE NUMBER FOR DATA STORAGE: ";1600 INPUT F1610 PRINT "IS THIS A NEW DATA FILE? (Y OR N): ";1620 INPUT G$1630 IF G$="N" THEN 17201640 FIND @P3,27