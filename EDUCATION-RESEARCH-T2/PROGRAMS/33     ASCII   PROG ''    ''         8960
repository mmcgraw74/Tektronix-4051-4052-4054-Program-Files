:::: P = Standard Pause Time Unit [*]1150 P=1.21180 REM:::: H0 = Heat Flag=> 1:No Heat  2:Start  3:Heat 1190 H0=11200 REM:::: L5 = Voltage Limit on V1: determine Heat cycle [*]1210 L5=0.051220 REM:::: H8 = Current Heating Index1230 H8=01240 REM:::: R8 = Current Resistance Change Index1250 R8=01280 REM:::: Pi = % of Unit Pause: P1:fast P2:med P3:slow [*]1290 P1=0.11300 P2=0.51310 P3=11315 P4=0.151320 REM:::: P0 = Pause Length (as % of P)1330 P0=P*P21340 REM:::: M2 = Size of Extrapolation Vectors [*]1350 M2=4001500 DELETE T1,T2,W1,W2,R11600 REM:::: W1, W2 = Mean V1, V2 during Heating # H81610 REM:::: T1 = Time at Start of Heating # H81620 REM:::: W0 = heat dV1622 REM:::: W3 = heat Ri1624 REM:::: W4 = heat Rf1630 DIM W0(H9),W1(H9),W2(H9),W3(H9),W4(H9),T1(H9)1632 W0=01634 W1=01636 W2=01638 W3=01640 W4=01642 T1=01670 REM:::: R1 = Resistance Value at R-change # R81680 DIM R1(R9)1690 R1=01700 REM:::: T2 = Time (approx) at R-change # R81710 DIM T2(R9)1720 T2=01722 REM1723 REM:::: Q7=Device where data is stored (33=4051, 2=4924)1724 REM:::: Q8=Device where program taped is placed1725 Q7=331726 Q8=21730 REM:::: G1=Start Plot (time)1740 REM:::: G2= Stop Plot (time)1750 REM:::: G3=  Max Plot (time)1760 REM:::: G4= Tic Interval1770 REM:::: G5= Loc. of Tape (time)1780 REM:::: T4 = Threshold factor for det exp heat start1790 T4=01800 REM:::: N6 = Max size of experiment vectors1810 N6=4001820 REM:::: N7 = Actual size of exp vectors1830 REM:::: O1 = Plot Device1840 O1=321850 REM:::: O2 = Sec. Addr.1860 REM:::: O6 = Length of Cal. Heat1870 O6=501880 REM:::: O4 = max no. chart cal's1890 O4=121900 REM:::: O5 = curr.no. chart cal's.1905 O5=01906 REM:::: O7 = Index of CC under examination1907 O7=01910 REM:::: W5 = C.C. Ri1912 REM:::: W6 = C.C. Rf1914 REM:::: W7 = C.C. dV1916 REM:::: T7 = time @ C.C.1918 DIM W5(O4),W6(O4),W7(O4),T7(O4)1920 W5=01922 W6=01924 W7=01926 T7=01930 REM:::: F9 = Max Num of files on Data Tape1935 F9=141940 REM:::: F8 = Current File of Data Tape1942 F8=11945 REM:::: T3 = Time at Start of this File1946 DIM T3(F9)1948 T3=01949 REM:::: F7 = Reaction Flag1950 F7=01954 REM:::: V6,V7,V8,V9 = Reaction Memories1955 V6=01956 V7=01957 V8=01958 V9=01960 REM:::: N1 = Max number of DATA PTS. (T,V) per file1961 N1=10001962 REM:::: T8 = Factor to add to T(1/2) in Heatings1963 T8=51990 REM[][][][][][][][][][][][][][][][][][][][][][][][]2000 REM2010 REM2020 REM        INITIALIZATION2030 REM[][][][][][][][][][][][][][][][][][][][][][][][]2040 DELETE 10000,320002100 F8=22105 FIND @Q7:F82110 SET KEY 2120 REM2130 T0=02140 CALL "TIMSET",02150 DIM A$(12),B$(10),C$(20),D$(1),F$(20)2160 T5=02170 T3(1)=02180 I0=02240 PAGE 2245 PRINT "Enter Sample ID: ";2246 INPUT F$2250 PRINT "Enter Date: ";2255 INPUT C$2260 T0=02800 PRINT "Ready.  Press User-Definable #1."2900 WAIT 2990 REM[][][][][][][][][][][][][][][][][][][][][][][][]3000 REM3010 REM3020 REM        INPUT SECTION3030 REM[][][][][][][][][][][][][][][][][][][][][][][][]3040 IF T0=999 THEN 31003050 CALL "TIMSET",T03100 CALL "PAUSE",P03110 WBYTE @41,122:67,49,49,693120 INPUT @3:V3130 CALL "TIME",T03140 WRITE @Q7:T0,V3150 I0=I0+13160 IF I0<N1 THEN 32103170 WRITE @Q7:0,03172 F8=F8+13173 CALL "TIME",T03174 IF F8>F9 THEN 31903175 T3(F8)=T03176 FIND @Q7:F83178 I0=03180 GO TO 32103190 PRINT "END OF DATA TAPE"3191 END 3210 GO TO H0 OF 3100,3300,34003215 REM ^^ Check Heat Flag ^^3220 GO TO 90403300 REM::::Watch V1 for Start of Heat Cycle::::3302 WBYTE @41,122:67,49,50,693304 INPUT @3:V13310 IF V1>L5 THEN 33203312 WBYTE @41,122:67,49,50,693314 INPUT @3:V13316 IF V1<L5 THEN 31103320 REM::::Start Heat!::::3330 H0=33340 S1=03350 S2=03360 I=03370 H8=H8+13380 T1(H8)=T03390 P0=P*P33400 REM::::Heating::::3410 REM::::Chk for End of Heat Cycle::::3411 FOR J=1 TO 23412 WBYTE @41,122:67,49,50,693414 INPUT @3:V13420 IF V1<L5 THEN 36003425 NEXT J3430 WBYTE @41,122:67,49,51,693440 INPUT @3:V23450 REM::::(S1=V1 sum; S2=V2 sum; I=ctr)::::3460 S1=S1+V13470 S2=S2+V23480 I=I+13490 WBYTE @41,122:67,49,50,693492 INPUT @3:V13494 IF V1>L5 THEN 31103600 REM::::End of Heat::::3610 REM::::Compute Mean of V1,V2 => W1,W2::::3620 W1(H8)=S1/I3630 W2(H8)=(S2-V2)/(I-1)3635 PRINT "EMF(1):",W2,"EMF(2):",W13640 P0=0.53650 H0=13670 GO TO 31003990 REM[][][][][][][][][][][][][][][][][][][][][][][][]4000 REM:::: STOP THE CLOCK! (and Data Coll.)4010 CALL "TIME",T04015 PRINT USING 4020:T04020 IMAGE "Duration of Run = ",FD.2D," seconds"4025 PRINT "Data stored through Tape File ";F84026 PRINT "To Resume, FIND ";F8+1;" then Press #1"4030 END 4100 REM::::      (PAUSE)4110 T0=9994120 WAIT 4300 REM::::U.D.K.#11..Close & Overlay Plot ::::4310 WRITE @Q7:0,04312 FIND @Q7:14313 WRITE @Q7:F$4314 WRITE @Q7:C$4320 WRITE @Q7:H84330 IF H8=0 THEN 43704340 FOR I=1 TO H84350 WRITE @Q7:T1(I)4360 NEXT I4370 WRITE @Q7:R84380 IF R8=0 THEN 44114390 FOR I=1 TO R84400 WRITE @Q7:T2(I),R1(I)4410 NEXT I4411 WRITE @Q7:F94412 FOR I=1 TO F94414 WRITE @Q7:T3(I)4416 NEXT I4420 FOR I=1 TO H84430 WRITE @Q7:W1(I),W2(I)4440 NEXT I4450 WRITE @Q7:O54460 FOR I=1 TO O54470 WRITE @Q7:T7(I),W5(I),W6(I)4480 NEXT I4485 WRITE @Q7:V6,V7,V84490 WRITE @Q7:T04500 G3=T04510 WRITE @Q7:04515 A=04520 GO TO 70006000 REM6010 REM           DATA FROM TAPE6020 REM[][][][][][][][][][][][][][][][][][][][][][][]6030 PAGE 6050 FIND @Q7:16060 FIND @Q8:36070 READ @Q7:F$6080 READ @Q7:C$6090 PRINT "   ";F$;"   ";C$;""6100 READ @Q7:H86110 IF H8=0 THEN 61506120 FOR I=1 TO H86130 READ @Q7:T1(I)6140 NEXT I6150 READ @Q7:R86160 IF R8=0 THEN 61916170 FOR I=1 TO R86180 READ @Q7:T2(I),R1(I)6190 NEXT I6191 READ @Q7:F96192 FOR I=1 TO F96194 READ @Q7:T3(I)6196 NEXT I6198 IF H8=0 THEN 63006200 FOR I=1 TO H86202 READ @Q7:W1(I),W2(I)6204 NEXT I6300 READ @Q7:O56305 IF O5=0 THEN 63406310 FOR I=1 TO O56320 READ @Q7:T7(I),W5(I),W6(I)6330 NEXT I6340 READ @Q7:V6,V7,V86350 READ @Q7:G36360 READ @Q7:A6370 IF A=0 THEN 70006400 REM***** Get Summary Info6410 FOR I=1 TO H86420 READ @Q7:W0(I),W3(I),W4(I)6430 NEXT I6440 FOR I=1 TO O56450 READ @Q7:W7(I),T7(I)6460 NEXT I6470 READ @Q7:V9,V67000 REM***** PREP FOR PLOT7010 F8=27020 FIND @Q7:F87030 G1=07040 G2=G37050 G5=T3(F8)7060 DELETE T07070 REM** Determine Ri & Rf for Heats **7075 IF A<>0 THEN 71707080 FOR I=1 TO H87090 FOR J=2 TO R87100 IF T1(I)<T2(J) THEN 71407110 NEXT J7120 PRINT "No R-change after heat #";I7130 GO TO 63507140 W4(I)=R1(J)7150 W3(I)=R1(J-1)7160 NEXT I7170 FIND @Q8:37180 GO TO 907190 REM[][][][][][][][][][][][][][][][][][][][][][]9000 REM9010 REM9020 REM       ERROR MESSAGES9030 REM[][][][][][][][][][][][][][][][][][][][][][][][]9040 PRINT "...Error"9060 END 9070 REM[][][][][][][][][][][][][][][][][][][][][][][][]