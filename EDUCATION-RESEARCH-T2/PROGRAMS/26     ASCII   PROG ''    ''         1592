810 R=N-2820 DIM V1(R*3-2),V2(R),V3(R*3-2),V4(R),H(L)830 FOR J=1 TO L840 H(J)=X(J+1)-X(J)850 NEXT J860 V1(1)=2*(H(2)+H(1))/H(2)870 V1(3)=1880 V2(1)=6/H(2)*((Y(3)-Y(2))/H(2)-(Y(2)-Y(1))/H(1))890 FOR J=2 TO R900 K=3*J910 V1(K-4)=H(J)/H(J+1)920 V1(K-2)=2*(H(J+1)+H(J))/H(J+1)930 IF J=R THEN 950940 V1(K)=1950 V2(J)=6/H(J+1)*((Y(J+2)-Y(J+1))/H(J+1)-(Y(J+1)-Y(J))/H(J))960 NEXT J970 V3(1)=V1(1)980 K=2990 J=21000 V3(K)=V1(K)1010 V3(K+1)=V1(K+1)/V3(K-1)1020 V3(K+2)=V1(K+2)-V3(K)*V3(K+1)1030 J=J+11040 K=K+31050 IF J<=R THEN 10001060 V4(1)=V2(1)/V3(1)1070 FOR K=2 TO R1080 V4(K)=(V2(K)-V3(3*K-4)*V4(K-1))/V3(3*K-2)1090 NEXT K1100 FOR K=2 TO R1110 J=R-K+11120 V4(J)=V4(J)-V3(3*J)*V4(J+1)1130 NEXT K1140 A=01150 FOR J=1 TO L1160 IF J=1 THEN 11801170 A=B1180 B=01190 IF J=L THEN 12101200 B=V4(J)1210 C=Y(J+1)/H(J)-H(J)*B/61220 D=Y(J)/H(J)-H(J)*A/61230 A0(J)=(A*X(J+1)^3-B*X(J)^3)/(6*H(J))-C*X(J)+D*X(J+1)1240 A1(J)=(B*X(J)^2-A*X(J+1)^2)/(2*H(J))+C-D1250 A2(J)=(A*X(J+1)-B*X(J))/(2*H(J))1260 A3(J)=(B-A)/(6*H(J))1270 NEXT J1280 GO TO 89