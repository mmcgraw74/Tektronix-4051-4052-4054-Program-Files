O" OR Z$="PR" THEN 54725470 GO TO 54205472 IF Z$="PO" THEN 54795473 Z$="Y"5474 GO TO 54805479 Z$="N"5480 E$=E$&Z$5490 REM SAVE DATA TO TAPE5500 PRINT "SAVING DATA TO TAPE"5505 IF D0<240 THEN 56005506 FIND 15507 WRITE @33:D0,D15510 GOSUB 60305600 FIND D0+25610 WRITE @33:E$5620 D0=D0+15625 D1(D0)=H5630 PRINT "DO YOU WANT TO ENTER ANOTHER PATIENT'S DATA? (Y/N) ";5640 INPUT Z$5641 Z$=SEG(Z$,1,1)5642 IF Z$="N" THEN 57405643 IF Z$="Y" THEN 245644 GO TO 56305740 REM UPDATE INDEX (FILE 1 ON DATA TAPE)5750 FIND 15760 WRITE @33:D0,D15770 CLOSE 5780 END 6000 FIND 16010 READ @33:D06020 IF D0<240 THEN 60706030 PRINT "DATA TAPE IS FULL.  INSERT ANOTHER DATA "6040 PRINT "STORAGE TAPE AND PRESS RETURN.";6050 INPUT Z$6060 GO TO 60006070 D1=06080 READ @33:D16090 RETURN 8000 REM8010 PRINT "THIS WILL GENERATE A NEW RAW DATA TAPE."8020 PRINT "TYPE THE WORD 'YES' WHEN THE TAPE TO BE FORMATTED"8030 PRINT "HAS BEEN INSERTED INTO THE MACHINE.  REMEMBER THAT"8040 PRINT "ANYTHING ON THE TAPE WILL BE DESTROYED....SO THIS IS"8050 PRINT "YOUR LAST CHANCE TO SAVE THIS TAPE"8055 PRINT "THIS WILL TAKE SEVERAL MINUTES TO PERFORM!!"8060 INPUT Z$8070 IF Z$<>"YES" THEN 80108080 FIND 08090 MARK 1,30008100 MARK 240,1008110 D0=08115 D1=08120 GO TO 57509000 REM CONVERT TO INPUT FOR STAT 49010 PAGE 9020 PRINT "PLEASE INSERT DATA TAPE AND PRESS RETURN"9030 INPUT T$9040 FIND 19050 READ @33:N9060 N2=N+19070 PAGE 9075 DELETE T$,R1,R29080 DIM T$(1),R1(N),R2(N)9090 E$=""9100 PRINT "PLEASE SELECT THE TWO DATA ITEM NUMBERS TO BE COMPARED"9110 PRINT " 1. AGE		17. B.P.(1)"9120 PRINT " 2. SEX		18. B.P.(2)"9130 PRINT " 3. DIABETIC		19. B.P.(3)"9140 PRINT " 4. SMOKER		20. B.P.(4)"9150 PRINT " 5. STD B.P.		21. B.P.(5)"9160 PRINT " 6. HEALED/FAILED(1)	22. B.P.(6)"9170 PRINT " 7. HEALED/FAILED(2)	23. B.P.(7)"9180 PRINT " 8. DOPPLER FLOW		24. Tcpo2(1)"9190 PRINT " 9. RATIO(1)		25. Tcpo2(2)"9200 PRINT "10. RATIO(2)	