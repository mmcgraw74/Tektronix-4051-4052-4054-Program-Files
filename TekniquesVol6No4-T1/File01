1 REM AUTOLOAD DIRECTORY
2 GO TO 100
4 GO TO 160
100 INIT 
110 DATA 17,2,3,4,6,7,8,9,10,11,12,14,15,16,18,19,23,28
120 READ N
130 IMAGE 10T,2D,5X,FA
140 DIM F(N)
150 READ F
160 PRINT "       4050 APPLICATIONS LIBRARY PROGRAMS
"
170 PRINT "            TEKniques Vol. 6 No.4 T1 Tape
"
180 PRINT "      PROGRAM   TITLE
"
190 PRINT USING 130:1,"4052A/54A Assembler"
200 PRINT USING 130:2,"TECO"
210 PRINT USING 130:3,"PCB Layout"
220 PRINT USING 130:4,"Pipe Construction"
230 PRINT USING 130:5,"Moment of Inertia"
240 PRINT USING 130:6,"FFT of 2048 Real Numbers"
250 PRINT USING 130:7,"IFT of 1024 Complex Numbers"
260 PRINT USING 130:8,"Newton Integration and Plot"
270 PRINT USING 130:9,"Bauer-Reinsch Inversion"
280 PRINT USING 130:10,"4050-4010 Utilities"
290 PRINT USING 130:11,"Telex Tape"
300 PRINT USING 130:12,"Data Chart"
310 PRINT USING 130:13,"SDBAR"
320 PRINT "
CAUTION: TRANSFER EACH OF THE FOLLOWING PROGRAMS "
330 PRINT "TO ITS OWN DEDICATED TAPE.  SEE DOCUMENTATION.
"
340 PRINT USING 130:14,"4050/468 Utility III"
350 PRINT USING 130:15,"Inventory Control"
360 PRINT USING 130:16,"Flowcharter II"
370 PRINT USING 130:17,"Friendly Graphing"
380 PRINT "
ENTER THE PROGRAM OF YOUR CHOICE OR 0 TO END. ";
390 INPUT P
400 P=INT(P)
410 IF P=0 THEN 550
420 IF P<1 OR P>N THEN 160
430 IF P<14 THEN 480
440 PRINT "
YOU CAN'T RUN THAT PROGRAM FROM THIS TAPE; YOU MUST "
450 PRINT "TRANSFER IT TO ITS OWN DEDICATED TAPE."
460 PRINT "
PRESS USER-DEFINABLE KEY 1 TO CHOOSE ANOTHER"
470 RETURN 
480 FIND F(P)
490 T=TYP(0)
500 IF T>2 THEN 530
510 FIND F(P)
520 OLD 
530 FIND F(P)
540 CALL "BOLD"
550 PRINT "
Goodbye.  If you change your mind, press UDK #1"
560 END 
