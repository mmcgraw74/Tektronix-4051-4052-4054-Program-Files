100 INIT 110 DIM Y$(8192),Y(2,2)120 PAGE130 F=21140 X0=13150 Y0=0160 FIND @5:F170 REMARK  READ AND DRAW GRAPHICS STRING up to "|EOG|" into Y$180 READ @5:Y$190 IF Y$="|EOG|" THEN 220200 CALL "RDRAW",Y$,1,X0,Y0210 GO TO 180220 IF F=25 THEN 270230 F=25240 X0=18250 Y0=78 260 GO TO 160270 HOME280 END 