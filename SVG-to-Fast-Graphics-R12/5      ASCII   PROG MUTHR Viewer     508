100 INIT 110 D$=""120 DIM Y$(8192),Y(2,2),D$(6)130 REM PRINT "Input R12 File#: ";132 PAGE135 F=21136 X0=13137 Y0=0140 FIND @5:F145 GO TO 180150 INPUT @5,19:A$160 A$=SEG(A$,21,16)170 PRINT "";A$180 FIND @5:F190 REMARK         GET GRAPHICS STRING up to "|EOG|" into J$200 READ @5:Y$210 IF Y$="|EOG|" THEN 260230 CALL "RDRAW",Y$,1,X0,Y0240250 GO TO 200260 IF F=25 THEN 320270 F=25280 X0=18290 Y0=78 300 D$=""310 GO TO 180320 HOME330 END 