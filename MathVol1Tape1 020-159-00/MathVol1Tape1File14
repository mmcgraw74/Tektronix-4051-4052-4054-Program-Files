1000 REM  ******RESIDENT OVERLAY******
1010 REM  *********ENTRY/CORRECT ETC.********** 
1020 GO TO P9 OF 1090,1490,1680
1030 IF P9<14 THEN 1060
1040 PRINT "J_DRAW A GRAPH BEFORE REQUESTING AN AXIS"
1050 END 
1060 GOSUB 1760
1070 GO TO 100
1080 REM  ****INPUT DATA*** 
1090 DELETE X,Y
1100 DIM P(10),Q(8),P$(10),Q$(1)
1110 PRINT "ENTER AN UPPER BOUND ON THE NUMBER OF DATA PAIRS = ";
1120 INPUT P(1)
1125 IF P(1)<4 THEN 1920
1130 SET KEY 
1140 DIM X(P(1)),Y(P(1))
1150 Q0=100
1160 Q9=32
1170 PRINT "J_ARE THE XI'S EVENLY SPACED: ";
1180 INPUT Q$
1190 IF Q$="Y" THEN 1340
1200 IF Q$<>"N" THEN 1170
1210 FOR N=0 TO P(1)-1
1220 P1=N+1
1230 PRINT "J_";P1,"ENTER X,Y = ";
1240 INPUT P3,Y(P1)
1250 IF P1=1 THEN 1310
1260 FOR P2=1 TO N
1270 IF P3<>X(P2) THEN 1300
1280 PRINT "ERROR: X = X(";P2;")";
1290 GO TO 1230
1300 NEXT P2
1310 X(P1)=P3
1320 NEXT N
1330 GO TO 1430
1340 PRINT "J_ENTER X MIN AND STEP SIZE = ";
1350 INPUT X(1),P3
1360 FOR N=0 TO P(1)-1
1370 P1=N+1
1380 PRINT "J_";P1,"X = ";X(P1),"ENTER Y = ";
1390 INPUT Y(P1)
1400 IF P1=>P(1) THEN 1420
1410 X(P1+1)=X(P1)+P3
1420 NEXT N
1430 PRINT "J_";P(1);" DATA PAIRS ENTERED - THE MAXIMUM REQUESTED"
1440 N=P(1)
1450 END 
1460 REM  ****END OF DATA ENTRY**** 
1470 REM
1480 REM  ****CORRECT DATA****
1490 PRINT "J_J_CORRECT THE DATAJ_"
1500 PRINT "J_ENTER POINT# = ";
1510 INPUT P1
1520 PRINT "PRESENT VALUE    = ";X(P1),Y(P1)
1530 PRINT "ENTER NEW VALUES = ";
1540 INPUT P4,P5
1550 FOR P2=1 TO N
1560 IF P2=P1 THEN 1610
1570 IF X(P2)<>P4 THEN 1610
1580 PRINT "ERROR:  NEW VALUES NOT ACCEPTED - NEW X = X(";P2;")"
1590 PRINT "TRY AGAINJ_"
1600 GO TO 1520
1610 NEXT P2
1620 X(P1)=P4
1630 Y(P1)=P5
1640 GO TO 1500
1650 REM  ****END OF CORRECT DATA**** 
1660 REM
1670 REM  ****PRINT DATA****
1680 PRINT "I	XI	YI"
1690 FOR P1=1 TO N
1700 PRINT "J_";P1,X(P1),Y(P1)
1710 NEXT P1
1720 END 
1730 REM  ****END OF PRINT DATA**** 
1740 REM
1750 REM  ****BUBBLE SORT AND PREPARE FOR PLOTS**** 
1760 P2=N+1
1770 P3=0
1780 FOR P1=1 TO P2-2
1790 IF X(P1)<=X(P1+1) THEN 1870
1800 P3=P1+1
1810 P4=X(P1)
1820 X(P1)=X(P3)
1830 X(P3)=P4
1840 P4=Y(P1)
1850 Y(P1)=Y(P3)
1860 Y(P3)=P4
1870 NEXT P1
1880 P2=P3
1890 IF P3<>0 THEN 1770
1900 P0=(X(N)-X(1))/Q0
1910 IF N=>4 THEN 1940
1920 PRINT "J_MINIMUM OF 4 DATA PAIRS REQUIRED, TRY AGAIN";
1925 PRINT "";
1930 GO TO 4
1940 RETURN 
1950 REM  ****END OF BUBBLE SORT****
1960 REM





