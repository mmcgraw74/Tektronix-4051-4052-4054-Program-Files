5000 REM FILE 195010 DELETE A5020 SET DEGREES 5030 PAGE 5040 DIM A(3)5050 DATA 50,20,2,5,95060 DATA 205,335,195,345,180,3555070 READ X,Y,A5080 FOR I=1 TO 35090 READ L,M5100 MOVE X+COS(L)*A(I),Y+SIN(L)*A(I)5110 FOR J=L TO M STEP 35120 DRAW X+COS(J)*A(I),Y+SIN(J)*A(I)5130 NEXT J5140 NEXT I5150 VIEWPORT X-20,X+20,Y-20,Y-1.25160 WINDOW X-20,X+20,Y-20,Y-1.25170 FOR J=545 TO 720 STEP 55180 ROPEN J5190 MOVE X+COS(J)*A(1),Y+SIN(J)*A(1)5200 DRAW X+COS(J)*A(2),Y+SIN(J)*A(2)5210 BLINK J,2*0.02667,2*0.026675220 RCLOSE 5230 NEXT J5240 VIEWPORT 0,130,0,1005250 WINDOW 0,130,0,1005260 BRIGHTNESS 05270 MOVE X+3,Y-15280 ROTATE 05290 RDRAW -(A(3)-1),05300 FOR I=0 TO 200 STEP 55310 ROTATE -I5320 RDRAW -0.61,05330 NEXT I5340 ROTATE -I5350 RDRAW -2.5*A(3),05360 FOR I=I TO I+160 STEP 55370 ROTATE -I5380 RDRAW -0.2,05390 NEXT I5400 ROTATE 05410 DRAW X,Y-15420 MOVE 130,Y-A(3)5430 DRAW 0,Y-A(3)5440 X=X-1.7445450 Y=Y+125460 MOVE X,Y5470 RDRAW 0,55480 RDRAW -0.5,05490 RDRAW 0,0.55500 RDRAW 5.5,05510 GIN X,Y5520 RDRAW 0,-0.55530 RDRAW -0.5,05540 RDRAW 0,-15550 RDRAW 1,05560 FOR I=0 TO 180 STEP 105570 ROTATE -I5580 RDRAW 0.2,05590 NEXT I5600 ROTATE 05610 RDRAW -1,05620 RDRAW 0,-45630 MOVE X-1,Y5640 ROPEN 4005650 RDRAW 1,05660 RDRAW 0,0.55670 RDRAW -0.5,05680 RDRAW 0,15690 RDRAW 1,05700 FOR J=0 TO 180 STEP 105710 ROTATE +J5720 RDRAW 0.2,05730 NEXT J5740 ROTATE 05750 RDRAW -1,05760 RDRAW 0,45770 RDRAW 5,55780 RDRAW 90,55790 RMOVE -150,05800 RDRAW 45.5,-55810 RDRAW 5,-55820 RDRAW 0,-7.355830 RDRAW -0.5,05840 RDRAW 0,-0.55850 RDRAW 1,05860 RDRAW 3.5,05870 RCLOSE 5880 PRINT @32,21:65,285890 CHARSIZE 45900 PRINT "AIRPLANE WHEEL ASSEMBLY SIMULATION";5910 B=05920 P=15930 C=05940 H=SIN(B)*5+55950 ROPEN 800+C5960 VISIBILITY 800+C,05970 MOVE X-1,Y5980 RDRAW 0,H+55990 RMOVE -3.5,06000 RDRAW 0,-(H+5)6010 RMOVE 4.5+0.64,-2.566020 D=(H+5+2*2.56)/26030 E=ASN(D/15)6040 ROTATE +E6050 RMOVE 1,06060 RDRAW 14,06070 ROTATE -E6080 RDRAW -14,06090 RMOVE -0.4,16100 RDRAW 15.5,06110 RDRAW 0,-16120 RDRAW -1.5,06130 RMOVE 1.5,06140 ROTATE E6150 RDRAW -15.2,06160 ROTATE 06170 RCLOSE 6180 VISIBILITY 800+C,16190 VISIBILITY 800+NOT(C),06200 STPOINT 400,X-1,Y+5+H6210 B=(B<360)*B+106220 C=NOT(C)6230 P=P+16240 IF P=100 THEN 62606250 GO TO 59406260 RINIT 6270 DELETE A6280 GO TO 9064554 CALL "BSAVE"64560 OLD "DISCTOTAPE.CONT"