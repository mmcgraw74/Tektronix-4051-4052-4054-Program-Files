5000 REM FILE 21 CLOCK5010 PAGE 5020 RINIT 5030 RESTORE 50405040 DATA 2.5,3,47,48,505050 SET DEGREES 5060 FOR I=1 TO 55070 READ R5080 MOVE 65,50+R5090 FOR J=6 TO 360 STEP 65100 DRAW 65+SIN(J)*R,50+COS(J)*R5110 NEXT J5120 NEXT I5130 FOR I=1 TO 605140 X=SIN(360/60*I)5150 Y=COS(360/60*I)5160 MOVE 65+X*45,50+Y*455170 DRAW 65+X*46,50+Y*465180 NEXT I5190 CHARSIZE 45200 FOR I=1 TO 125210 X=SIN(360/12*I)5220 Y=COS(360/12*I)5230 MOVE 65+X*42,50+Y*425240 A$=STR(I)5250 A$=REP("",1,1)5260 RMOVE -LEN(A$)*2,-1.55270 PRINT I;5280 NEXT I5290 CHARSIZE 25300 A$="TEKTRONIX"5310 PRINT @32,21:60,405320 PRINT A$;5330 CHARSIZE 15340 H=35350 M=605360 S=305370 C=05380 K=1+C5390 J=1+NOT(C)5400 C=NOT(C)5410 ROPEN K5420 VISIBILITY K,05430 R=360/12*H+30*M/60+905440 R1=360/60*M+90+6*S/605450 R0=360/60*S+905460 ROTATE 90-R05470 MOVE 65,505480 RMOVE 0,35490 RDRAW 0,405500 RDRAW 1,05510 RDRAW -1,25520 RDRAW -1,-25530 RDRAW 1,05540 ROTATE 90-R15550 L=1.55560 Q=0.35570 GOSUB 57405580 ROTATE 90-R5590 L=15600 Q=0.55610 GOSUB 57405620 RCLOSE 5630 VISIBILITY K,15640 VISIBILITY J,05650 S=S*(S<=60)+15660 M=M+(S=1)5670 M=M*(M<=60)+(M=61)5680 H=H+(M=1)5690 H=H*(H<=12)+(H=13)5700 IF M=1 THEN 59705710 FOR I=1 TO 13005720 NEXT I5730 GO TO 53805740 REM HANDS OF CLOCK DRAW ROUTINE5750 MOVE 65,505760 RMOVE -1,35770 RDRAW 0,5*L5780 RDRAW -2*Q,15790 RDRAW -2*Q,2*L5800 RDRAW -1*Q,2*L5810 RDRAW 0,2*L5820 RDRAW 1*Q,2*L5830 RDRAW 2*Q,2*L5840 RDRAW 2*Q,3*L5850 RDRAW 0.5*Q,5*L5860 RDRAW 1,05870 RDRAW 0.5*Q,-5*L5880 RDRAW 2*Q,-3*L5890 RDRAW 2*Q,-2*L5900 RDRAW 1*Q,-2*L5910 RDRAW 0,-2*L5920 RDRAW -1*Q,-2*L5930 RDRAW -2*Q,-2*L5940 RDRAW -2*Q,-15950 RDRAW 0,-5*L5960 RETURN 5970 REM5980 RINIT 5990 GO TO 9064554 CALL "BSAVE"64560 OLD "DISCTOTAPE.CONT"