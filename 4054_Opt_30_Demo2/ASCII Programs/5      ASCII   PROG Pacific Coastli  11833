5000 REM FILE 5 MAP 2/24/77 CURE'     10/5/79 NOLLETTE5010 VIEWPORT 5,127,3,1005020 FONT 55030 CHARSIZE 15040 GOSUB 59305050 RESTORE 70305060 DELETE X,Y5070 DIM X(491),Y(491)5080 PRINT "	MAP OF THE PACIFIC COASTINE
THE FOLLOWING KEYSTROKES ";5090 PRINT "CAUSE THESE ACTIONS;
Z=ZOOM LOWER LEFT THEN UPPER RIGHT
"5100 PRINT "R=REDRAW MAP AT FULL SCALE
S=STOP 
"5110 PRINT "P=PRINT LAT.LONG. NEXT TO POINT
L=SEARCH & DISPLAY POINT"5120 PRINT "
T=TARGET DISPLAY"5130 PRINT "

WHEN YOU'RE READY PRESS [RETURN]";5140 READ X,Y5150 INPUT Q$5160 PRINT @32,26:15170 VIEWPORT 5,127,3,1005180 RESTORE 51905190 DATA -200,-70,0,655200 READ X1,X2,Y1,Y25210 PAGE 5220 WINDOW X1,X2,Y1,Y25230 GOSUB 60605240 R1=X15250 R2=X25260 R3=Y15270 R4=Y25280 FOR J=0 TO 150 STEP 105290 AXIS 0,0,-200+J,J5300 NEXT J5310 FOR J=10*INT(X1*0.1) TO 10*INT(X2*0.1) STEP 105320 IF J<X1 OR J>X2+10 THEN 53505330 MOVE J,Y15340 PRINT "
";ABS(J);5350 NEXT J5360 FOR J=10*INT(Y1*0.1) TO 10*INT(Y2*0.1) STEP 105370 MOVE X1,J5380 PRINT "";J;5390 NEXT J5400 FOR J=1 TO 4915410 IF X(J)<0 THEN 54505420 X0=-X(J)5430 MOVE X0,Y(J)5440 GO TO 54605450 DRAW X(J),Y(J)5460 NEXT J5470 PRINT ""5480 POINTER X1,Y1,P$5490 VISIBILITY 101,05500 REM   POINTER                   R    P    S    T    I    L    Z5510 REM                             |    |    |    |    |    |    |5520 GO TO POS("RPSTILZ",P$,1) OF 5170,5870,5770,6600,6700,6750,55505530 GO TO 54805540 REM ZOOM ROUTINE5550 PRINT ""5560 MOVE X1-10,Y15570 RDRAW 20,05580 MOVE X1,Y1-105590 RDRAW 0,205600 POINTER X2,Y2,P$5610 IF P$="M" THEN 51705620 IF ABS(X2-X1)<0.5 OR P$<>"Z" THEN 55505630 MOVE X2-10,Y25640 RDRAW 20,05650 MOVE X2,Y2-105660 RDRAW 0,205670 PRINT ""5680 CALL "wait",15690 FOR J=1 TO 1005700 MOVE X1,Y15710 DRAW X2,Y15720 DRAW X2,Y25730 DRAW X1,Y25740 DRAW X1,Y15750 NEXT J5760 GO TO 52105770 REM TERMINATION ROUTINE5780 DELETE X,Y,X9,Y9,Z95790 U7=1005800 U8=805810 RINIT 5820 GO TO 905830 I=I+15840 IF I>491 THEN 54805850 GO TO 57905860 REM POINT ROUTINE5870 MOVE X1,Y15880 FOR J=1 TO 105890 DRAW X1,Y15900 NEXT J5910 PRINT USING "FD.DXFD.D":X1;Y15920 GO TO 54805930 REM gen orig targets5940 R5=15950 N=255960 DELETE X9,Y9,Z95970 DIM X9(N),Y9(N),Z9(N)5980 RESTORE 51905990 READ X1,X2,Y1,Y26000 WINDOW X1,X2,Y1,Y26010 FOR J=1 TO N6020 X9(J)=30*RND(-1)-1806030 Y9(J)=25*RND(-1)+256040 Z9(J)=INT(4*RND(-1)+1)6050 NEXT J6060 FOR J=1 TO N6070 MOVE X9(J),Y9(J)6080 ROPEN J6090 VISIBILITY J,06100 GOSUB 61406110 RCLOSE 6120 NEXT J6130 RETURN 6140 REM SYMBOL ROUTINE6150 S=0.756160 S2=S/26170 GO TO Z9(J) OF 6190,6270,6400,64706180 RETURN 6190 REM DIAMOND6200 RMOVE 0,S6210 RDRAW S2,-S6220 RDRAW -S2,-S6230 RDRAW -S2,S6240 RDRAW S2,S6250 RMOVE 0,-S6260 RETURN 6270 REM SAUCER6280 RMOVE 0,S26290 RDRAW S2,06300 RDRAW 0,-S26310 RDRAW S2,06320 RDRAW 0,-S26330 RDRAW -2*S,06340 RDRAW 0,S26350 RDRAW S2,06360 RDRAW 0,S26370 RDRAW S2,06380 RMOVE 0,-S26390 RETURN 6400 REM TRIANGLE6410 RMOVE 0,S26420 RDRAW S2,-S6430 RDRAW -S,06440 RDRAW S2,S6450 RMOVE 0,-S26460 RETURN 6470 REM VADERS6480 RMOVE 0,S26490 RDRAW S2,06500 RDRAW 0,S26510 RDRAW S2,06520 RDRAW 0,-S6530 RDRAW -2*S,06540 RDRAW 0,S6550 RDRAW S2,06560 RDRAW 0,-S26570 RDRAW S2,06580 RMOVE 0,-S26590 RETURN 6600 REM GENERATE DISPLAY OF TARGETS6610 FOR I=1 TO N6620 STPOINT I,X9(I),Y9(I)6630 IF X9(I)>R1 AND X9(I)<R2 AND Y9(I)>R3 AND Y9(I)<R4 THEN 66606640 VISIBILITY I,06650 GO TO 66706660 VISIBILITY I,16670 NEXT I6680 GO TO 54806690 REM INDEX TARGETS6700 FOR I=1 TO N6710 X9(I)=X9(I)+RND(-1)-0.26720 Y9(I)=Y9(I)+RND(-1)-0.26730 NEXT I6740 GO TO 66006750 REM SEARCH HOOK6760 L0=0.56770 I=16780 IF I>N THEN 70106790 IF ABS(X9(I)-X1)<L0 AND ABS(Y9(I)-Y1)<L0 THEN 68206800 I=I+16810 GO TO 67806820 BLINK I,0.5,0.56830 MOVE X9(I),Y9(I)6840 BLINK R5,0,06850 R5=I6860 P=06870 FOR J=1 TO Z9(I)6880 Q=P+16890 A$="MILITARY,COMMERCIAL,FOREIGN,UNKNOWN,"6900 P=POS(A$,",",Q)6910 NEXT J6920 A$=SEG(A$,Q,P-Q)6930 A$="    "&A$6940 B$=SEG("
",1,LEN(A$)-1)6950 A$=A$&B$6960 ROPEN 1016970 PRINT A$;6980 PRINT USING "FD.D"",""FD.DS":X9(I),Y9(I)6990 RCLOSE 7000 GO TO 54807010 VISIBILITY 101,07020 GO TO 54807030 DATA 162,-161.8,-161.7,-161.8,-161.87040 DATA -161.7,-162.1,-161.7,-160.3,-159.97050 DATA -159.4,-159.1,-158.8,-158.7,-158.77060 DATA -158.4,-158.6,-158.3,-157.7,-156.77070 DATA -157.5,-157.8,-158.6,-160.2,-160.57080 DATA -162,-162,-160.6,-159.4,-158.77090 DATA -157.9,-156.9,-156,-155.1,-154.17100 DATA -153.3,-154.2,-153.7,-153,-152.37110 DATA -151.8,-150.6,-150,-149,-150.37120 DATA -151.4,-151.9,-151.5,-151,-1527130 DATA -151.8,-150.2,-149.2,-147.9,-148.17140 DATA -146.9,-145.2,-143.9,-141.5,-140.27150 DATA -139.7,-139.5,-139.9,-138.7,-137.17160 DATA -136.1,-133.8,-132.7,-131.3,-1317170 DATA -130.5,-129,-128.2,-126.7,-128.37180 DATA -127.6,-126.5,-125,-122.7,-124.77190 DATA -124.6,-124,-123.9,-124.2,-124.67200 DATA -124.4,-124.5,-124.4,-124.2,-124.27210 DATA -124.1,-124.2,-124.1,-124.4,-124.47220 DATA -124.1,-123.8,-123.7,-123.8,-123.77230 DATA -123.7,-122.9,-123,-123,-122.97240 DATA -122.7,-122.5,-122.5,-122,-122.37250 DATA -122.4,-122.2,-122.4,-122.4,-122.57260 DATA -122.5,-122.4,-122.4,-122.4,-122.27270 DATA -122.1,-121.9,-121.8,-121.8,-121.97280 DATA -121.9,-121.5,-121.5,-121.3,-121.17290 DATA -121.1,-120.8,-120.9,-120.6,-120.77300 DATA -120.6,-120.7,-120.5,-119.6,-119.37310 DATA -119.2,-118.8,-118.5,-118.4,-118.57320 DATA -118.3,-118.1,-118,-117.8,-117.67330 DATA -117.3,-117.2,-117.3,-117.3,-117.17340 DATA -117.2,-117.2,-117,-116.9,-116.97350 DATA -116.6,-116.6,-116.7,-116.7,-116.37360 DATA -116.3,-116,-116,-115.9,-115.87370 DATA -115.9,-115.7,-115.4,-114.7,-114.17380 DATA -114.2,-114.4,-115.1,-114.9,-114.57390 DATA -114.4,-113.9,-113.7,-113.6,-113.47400 DATA -112.5,-112.4,-112.2,-112.1,-112.27410 DATA -112.3,-112,-111.8,-111.4,-1117420 DATA -110.7,-110.4,-110.2,-110,-109.77430 DATA -109.6,-109.4,-109.8,-109.8,-109.97440 DATA -