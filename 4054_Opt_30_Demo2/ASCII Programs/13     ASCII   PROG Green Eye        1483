5000 REM FILE 13 GREEN EYE 5010 CHARSIZE 25020 PAGE 5030 T=325040 PRINT @T,21:100,1005050 PRINT @T,20:130,100,130,80,100,80,100,1005060 PRINT @T:"


  ANY KEY WILL FIX IMAGE";5070 PRINT @T:"
";5080 PRINT @T:"
KEY `S' TO STOP"5090 PRINT @T,21:30,55100 CHARSIZE 45110 PRINT "UP TO 1000 CM. OF DYNAMIC VECTORS";5120 CHARSIZE 25130 SET DEGREES 5140 WINDOW -50,50,-50,505150 VIEWPORT 15,115,0,1005160 N=235170 Q0=125180 DELETE A,C,S5190 DIM A(N*(N-1)+2),C(N),S(N)5200 FOR I=1 TO N5210 S(I)=COS(I*360/N)*Q0-205220 C(I)=SIN(I*360/N)*Q0+205230 NEXT I5240 X=35250 V=05260 MOVE S(2),C(2)5270 GIN A(1),A(1+1)5280 ROPEN 15290 VISIBILITY 1,05300 FOR I=1 TO (N-1)/25310 FOR J=1 TO N5320 K=I*J-INT(I*J/N)*N+15330 A(X)=S(K)5340 A(X+1)=C(K)5350 RDRAW A(X-2)-A(X),A(X-1)-A(X+1)5360 V=V+15370 X=X+25380 NEXT J5390 NEXT I5400 RCLOSE 5410 CURSOR 15420 VISIBILITY 1,15430 POINTER X,Y,Z$5440 IF Z$="s" THEN 54705450 FIX 15460 GO TO 54305470 VISIBILITY 1,05480 DELETE A,C,S5490 GO TO 905500 LIST 5510,650005510 FOR I=2 TO 125520 C=0.35530 C1=05540 DELETE 5020,639905550 A$=STR(I)5560 A$=REP("@RD/DEMO",1,1)5570 APPEND A$;50105580 FIND I5590 MARK 1,SPACE*C5600 FIND I5610 ON EOF (0) THEN 56405620 CALL "bsave"5630 IF C1=0 THEN 56705640 C1=05650 C=C+0.15660 GO TO 55805670 PRINT "file number ";I;" has been COPIED at";C*100;"% OF SPACE"5680 NEXT I5690 END 5700 RETURN 64554 CALL "BSAVE"64560 OLD "DISCTOTAPE.CONT"