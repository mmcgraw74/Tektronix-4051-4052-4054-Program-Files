5000 REM FILE 205010 RINIT 5020 DELETE A,X5,Y5,P55030 SET DEGREES 5040 PAGE 5050 CHARSIZE 45060 PRINT "





	AIRPLANE LANDING GEAR REACTION SIMULATION"5070 DIM A(3),X5(130),Y5(130),P5(130)5080 FOR I=1 TO 1305090 X5(I)=I5100 NEXT I5110 DATA 50,20,2,5,95120 READ X,Y,A5130 BRIGHTNESS 05140 MOVE X+3,Y-15150 ROTATE 05160 ROPEN 4015170 VISIBILITY 401,05180 RDRAW -(A(3)-1),05190 FOR I=0 TO 200 STEP 55200 ROTATE -I5210 RDRAW -0.61,05220 NEXT I5230 ROTATE -I5240 RDRAW -2.5*A(3),-0.45250 FOR I=I TO I+160 STEP 55260 ROTATE -I5270 RDRAW -0.2,05280 NEXT I5290 ROTATE 05300 RDRAW -14,05310 RMOVE 3,05320 FOR I=0 TO -190 STEP -55330 ROTATE I5340 RDRAW 0,-0.655350 NEXT I5360 ROTATE 05370 RMOVE 5.5,12.55380 RDRAW 0,55390 RDRAW -0.5,05400 RDRAW 0,0.55410 RDRAW 5.5,05420 GIN X,Y5430 RDRAW 0,-0.55440 RDRAW -0.5,05450 RDRAW 0,-15460 RDRAW 1,05470 FOR I=0 TO 180 STEP 105480 ROTATE -I5490 RDRAW 0.2,05500 NEXT I5510 ROTATE 05520 RDRAW -1,05530 RDRAW 0,-3.85540 RCLOSE 5550 MOVE X-1,Y+155560 RDRAW 1,05570 RDRAW 0,0.55580 RDRAW -0.5,05590 RDRAW 0,15600 RDRAW 1,05610 FOR J=0 TO 180 STEP 105620 ROTATE +J5630 RDRAW 0.2,05640 NEXT J5650 ROTATE 05660 RDRAW -1,05670 RDRAW 0,45680 RDRAW 5,55690 RDRAW 90,55700 RMOVE -150,05710 RDRAW 45.5,-55720 RDRAW 5,-55730 RDRAW 0,-7.355740 RDRAW -0.5,05750 RDRAW 0,-0.55760 RDRAW 1,05770 RDRAW 3.5,05780 B=05790 P1=15800 C=05810 C1=17.15820 Y5=C15830 FOR J=0 TO 330 STEP 1205840 P9=B+J5850 IF P9<=360 THEN 58705860 P9=P9-3605870 P=INT(P9/360*130) MAX 15880 FOR I=P TO P+65890 IF I>130 THEN 59105900 Y5(I)=Y5(I)+SIN((I-P)*180/6)*25910 NEXT I5920 NEXT J5930 H=55940 FOR I=48 TO 525950 H=H+Y5(I)-C15960 NEXT I5970 H=H+(Y5(65)-C1)/3+(Y5(77)-C1)/65980 H=9-H5990 P5(P1)=H6000 MOVE 51.96,55.816010 ROPEN 800+P16020 VISIBILITY 800+P1,06030 RMOVE -1.64,-2.566040 RDRAW 0,-(H+5)6050 RMOVE -3.5,06060 RDRAW 0,+(H+5)6070 RMOVE 3.5+1.64,2.566080 D=(H+5+2*2.56)/26090 E=ASN(D/15)6100 ROTATE -E6110 RMOVE 1,06120 RDRAW 14,06130 ROTATE +E6140 RDRAW -14,06150 RMOVE -0.4,-16160 RDRAW 15.5,06170 RDRAW 0,16180 RDRAW -1.5,06190 RMOVE 1.5,06200 ROTATE -E6210 RDRAW -15.2,06220 MOVE X5(1),Y5(1)6230 DRAW X5,Y56240 ROTATE 06250 RCLOSE 6260 B=B+106270 P1=P1+16280 IF P1<>13 THEN 58006290 P1=P1-16300 VISIBILITY 401,16310 N=16320 P=P16330 Q=16340 VISIBILITY 800+Q,16350 VISIBILITY 800+P,06360 STPOINT 401,53,Y5(Q)-P5(Q)+126370 P=P*(P<P1)+16380 Q=Q*(Q<P1)+16390 N=N+16400 IF N=100 THEN 64306410 CALL "wait",0.26420 GO TO 63406430 RINIT 6440 DELETE A,X5,Y5,P56450 GO TO 9064650 CALL "BSAVE"64660 OLD "DISCTOTAPE.CONT"