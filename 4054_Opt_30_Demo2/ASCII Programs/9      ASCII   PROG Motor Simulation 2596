5000 REM FILE 9 MOTOR SIMULATION 5010 PAGE 5020 CHARSIZE 45030 PRINT "
	M E C H A N I C A L   S I M U L A T I O N";5040 N=15050 SET DEGREES 5060 WINDOW 130*0.2,130-130*0.2,30,905070 MOVE 70,505080 FOR I=0 TO 360 STEP 35090 DRAW 65+COS(I)*5,50+SIN(I)*55100 NEXT I5110 ROTATE 375120 MOVE 65,505130 RMOVE 6,05140 FOR I=36 TO -220 STEP -55150 ROTATE I5160 RDRAW 0,-0.545170 NEXT I5180 ROTATE 05190 RDRAW 0,55200 RDRAW 1.5,25210 RDRAW 0,165220 RDRAW 3,15230 GIN X1,Y15240 X1=X1+0.255250 RMOVE 0.5,05260 RDRAW 3,-15270 RDRAW 0,-165280 RDRAW 1.5,-25290 RDRAW 0,-55300 FOR I=1 TO 25310 MOVE 65,375320 RDRAW 8,05330 RDRAW 0,25340 RDRAW -3,05350 RDRAW 0,45360 RDRAW 1,05370 RDRAW 3,45380 RDRAW 0,55390 RDRAW -3,45400 RDRAW 0,75410 FOR J=1 TO 105420 RDRAW -2,05430 RDRAW 0,0.755440 RDRAW 2.1,05450 RDRAW 0.5,0.35460 RDRAW -0.5,0.35470 NEXT J5480 RDRAW 0,35490 FOR J=1 TO 45500 RDRAW -0.5,05510 RDRAW -0.5,-15520 RDRAW -0.5,05530 RDRAW 0,15540 NEXT J5550 RDRAW -0.5,-0.15560 MOVE X1,Y15570 RDRAW 0.6,05580 FOR J=1 TO 85590 RDRAW 0.1,0.15600 RDRAW -0.1,0.15610 NEXT J5620 RDRAW 0.5,05630 RDRAW 0,0.55640 RDRAW -0.5,0.55650 RDRAW 0,0.55660 RDRAW -0.1,05670 RDRAW -0.1,25680 RDRAW 0.1,05690 RDRAW 0,0.55700 RDRAW -0.15,05710 RDRAW 0,0.65720 RDRAW -0.3,0.25730 VIEWPORT 130,0,0,1005740 NEXT I5750 VIEWPORT 0,130,0,1005760 FOR I=0 TO 359 STEP 35770 ROPEN N5780 VISIBILITY N,05790 X=COS(I)*55800 Y=SIN(I)*55810 MOVE 65,505820 DRAW 65+X,50+Y5830 GIN A,B5840 DRAW 65,10+B+SQR(X^2+Y^2)5850 RDRAW 3,05860 RDRAW 0,65870 RDRAW -6,05880 RDRAW 0,-65890 RDRAW 3,05900 RMOVE -3.1,55910 RDRAW 6.2,05920 RMOVE 0,-0.45930 RDRAW -6.2,05940 RMOVE 0,-0.45950 RDRAW 6.2,05960 IF I<80 OR I>100 THEN 60105970 FOR J=60 TO 120 STEP 105980 MOVE X1+COS(J)*-0.2,Y1+SIN(J)*-0.25990 DRAW X1+COS(J)*-1.5,Y1+SIN(J)*-1.56000 NEXT J6010 RCLOSE 6020 N=N+16030 NEXT I6040 FOR I=1 TO N-16050 VISIBILITY I,16060 BLINK I,0.026*1,0.026*29.56070 NEXT I6080 ROPEN 20006090 PRINT @32,21:40,86100 CHARSIZE 46110 PRINT "PRESS ""RETURN"" KEY TO PROCEED";6120 RCLOSE 6130 INPUT A$6140 GO TO 906150 LIST 6160,650006160 FOR I=2 TO 126170 C=0.36180 C1=06190 DELETE 5040,639906200 A$=STR(I)6210 A$=REP("@RD/DEMO",1,1)6220 APPEND A$;50106230 FIND I6240 MARK 1,SPACE*C6250 FIND I6260 ON EOF (0) THEN 62906270 CALL "bsave"6280 IF C1=0 THEN 63206290 C1=06300 C=C+0.16310 GO TO 62306320 PRINT "file number ";I;" has been COPIED at";C*100;"% OF SPACE"6330 NEXT I6340 END 6350 RETURN 64554 CALL "BSAVE"64560 OLD "DISCTOTAPE.CONT"