5000 REM FILE 4 ZOOM DEMO5010 T=325020 F1=05030 RINIT 5040 ROTATE 05050 SET DEGREES 5060 FOR S=4.1 TO 0.5 STEP -0.035070 F1=F1+15080 MOVE 65,505090 SCALE S,S5100 GOSUB 53805110 WINDOW 0,130,0,1005120 NEXT S5130 PAGE 5140 PRINT @32,21:35,55150 CHARSIZE 45160 PRINT "FAST DYNAMIC TO STORAGE TRANSITION";5170 GOSUB 55105180 J1=0.085190 FOR J=1 TO 35200 FOR I=2 TO F1-15210 VISIBILITY I,15220 VISIBILITY I-1,05230 CALL "WAIT",J15240 NEXT I5250 FOR I=F1-2 TO 1 STEP -15260 VISIBILITY I,15270 VISIBILITY I+1,05280 CALL "WAIT",J15290 NEXT I5300 NEXT J5310 FOR I=1 TO F1-15320 FIX I5330 RDELETE I5340 NEXT I5350 HOME 5360 CHARSIZE 15370 GO TO 905380 REMARK STAR ROUTINE5390 ROPEN F15400 VISIBILITY F1,05410 ROTATE F1/25420 RMOVE 20,05430 RDRAW -36.1803398875,11.75570504595440 RDRAW 22.360679775,-30.77683537185450 RDRAW 4.547473509E-13,38.04226065185460 RDRAW -22.360679775,-30.77683537185470 RDRAW 36.1803398875,11.75570504595480 ROTATE 05490 RCLOSE 5500 RETURN 5510 FOR I=1 TO F1-15520 VISIBILITY I,15530 BLINK I,1*0.026,18*0.0265540 NEXT I5550 CALL "WAIT",105560 FOR I=1 TO F1-15570 VISIBILITY I,05580 BLINK I,0,05590 NEXT I5600 RETURN 5610 LIST 5620,650005620 FOR I=2 TO 125630 C=0.35640 C1=05650 DELETE 5020,639905660 A$=STR(I)5670 A$=REP("@RD/DEMO",1,1)5680 APPEND A$;50105690 FIND I5700 MARK 1,SPACE*C5710 FIND I5720 ON EOF (0) THEN 57505730 CALL "bsave"5740 IF C1=0 THEN 57805750 C1=05760 C=C+0.15770 GO TO 56905780 PRINT "file number ";I;" has been COPIED at";C*100;"% OF SPACE"5790 NEXT I5800 END 5810 RETURN 64554 CALL "BSAVE"64560 OLD "DISCTOTAPE.CONT"