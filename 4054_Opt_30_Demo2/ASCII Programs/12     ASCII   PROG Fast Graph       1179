5000 REM FILE 12 FAST GRAPH5010 GO TO 50405020 T=15030 GO TO 50505040 T=325050 SET DEGREES 5060 WINDOW 0,100,0,1005070 VIEWPORT 15,115,0,1005080 N=375090 DELETE A,C,S5100 DIM A(N*(N-1)),C(N),S(N)5110 FOR I=1 TO N5120 S(I)=SIN(I*360/N)*50+505130 C(I)=COS(I*360/N)*50+505140 NEXT I5150 X=15160 MOVE S(2),C(2)5170 FOR I=1 TO (N-1)/25180 FOR J=1 TO N5190 K=I*J-INT(I*J/N)*N+15200 A(X)=S(K)5210 A(X+1)=C(K)5220 MOVE A(X),A(X+1)5230 INPUT @32,24:A(X),A(X+1)5240 X=X+25250 NEXT J5260 NEXT I5270 PAGE 5280 CHARSIZE 45290 PRINT @T:"THE TEKTRONIX 4054"5300 PRINT @T:"BASIC PROGRAMMABLE"5310 PRINT @T:"FAST DISPLAYS"5320 PRINT @T:"FAST PROCESSING"5330 PRINT @T:"EASY TO USE"5340 PRINT @T,21:A(1),A(2)5350 PRINT @T,20:A5360 PRINT @T,20:A(1),A(2)5370 CHARSIZE 05380 DELETE C,S,A5390 U7=05400 U8=55410 REM---------------------------------------------------5420 REM W A R N I N G ! !5430 REM ^^^^^^^^^^^^^^^^^5440 REM    THIS MODULE DELETES PART OF ITS CODE---5450 REM D O   N O T   T E S T   B E F O R E   S A V I N G5460 REM5470 REM-----------------------------------------------LRN5480 GO TO 9064554 CALL "BSAVE"64560 OLD "DISCTOTAPE.CONT"